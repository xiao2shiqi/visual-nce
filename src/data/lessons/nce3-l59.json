  "segments": [
    {
      "id": "intro_1",
      "role": "Narrator",
      "text": "Lesson 59",
      "translation": "",
      "startTime": 0.33,
      "endTime": 2.39,
      "analysis": {
        "words": [
          {
            "word": "Lesson",
            "pos": "n.",
