    {
      "id": "intro_3",
      "role": "Narrator",
      "text": "Listen to the tape then answer this question.",
      "translation": "",
      "startTime": 6.85,
      "endTime": 12.59
    },
    {
      "id": "intro_4",
      "role": "Narrator",
      "text": "How must Amy clean the floor?",
      "translation": "",
      "startTime": 12.6,
      "endTime": 17.29
    },
    {
      "id": "s1",
      "role": "Man",
      "text": "Come in, Amy.",
      "translation": "",
      "startTime": 17.3,
      "endTime": 19.84
    },
    {
      "id": "s2",
      "role": "Woman",
      "text": "Shut the door, please.",
      "translation": "",
      "startTime": 19.85,
      "endTime": 22.48
    },
    {
      "id": "s3",
      "role": "Man",
      "text": "This bedroom's very untidy.",
      "translation": "",
      "startTime": 22.49,
      "endTime": 26.82
    },
    {
      "id": "s4",
      "role": "Narrator",
      "text": "What must I do, Mrs. Jones?",
      "translation": "",
      "startTime": 26.83,
      "endTime": 30.52
    },
    {
      "id": "s5",
      "role": "Man",
      "text": "Open the window and air the room.",
      "translation": "",
      "startTime": 30.53,
      "endTime": 34.91
    },
    {
      "id": "s6",
      "role": "Woman",
      "text": "Then put these clothes in the wardrobe.",
      "translation": "",
      "startTime": 34.92,
      "endTime": 39.68
    },
    {
      "id": "s7",
      "role": "Man",
      "text": "Then make the bed.",
      "translation": "",
      "startTime": 39.69,
      "endTime": 42.8
    },
    {
      "id": "s8",
      "role": "Woman",
      "text": "Dust the dressing table.",
      "translation": "",
      "startTime": 42.81,
      "endTime": 45.84
    },
    {
      "id": "s9",
      "role": "Man",
      "text": "Then sweep the floor.",
      "translation": "",
      "startTime": 45.85,
      "endTime": 50.85
    }
  ]
