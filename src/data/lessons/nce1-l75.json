    {
      "id": "intro_2",
      "role": "Narrator",
      "text": "Uncomfortable shoes",
      "translation": "",
      "startTime": 3.73,
      "endTime": 6.97
    },
    {
      "id": "intro_3",
      "role": "Narrator",
      "text": "Listen to the tape then answer this question.",
      "translation": "",
      "startTime": 6.98,
      "endTime": 12.37
    },
    {
      "id": "intro_4",
      "role": "Narrator",
      "text": "What's wrong with the fashionable shoes?",
      "translation": "",
      "startTime": 12.38,
      "endTime": 18
    },
    {
      "id": "s1",
      "role": "Man",
      "text": "Do you have any shoes like these?",
      "translation": "",
      "startTime": 18.01,
      "endTime": 22.07
    },
    {
      "id": "s2",
      "role": "Narrator",
      "text": "What size?",
      "translation": "",
      "startTime": 22.08,
      "endTime": 23.79
    },
    {
      "id": "s3",
      "role": "Man",
      "text": "Size five.",
      "translation": "",
      "startTime": 23.8,
      "endTime": 25.85
    },
    {
      "id": "s4",
      "role": "Narrator",
      "text": "What colour?",
      "translation": "",
      "startTime": 25.86,
      "endTime": 27.29
    },
    {
      "id": "s5",
      "role": "Man",
      "text": "Black.",
      "translation": "",
      "startTime": 27.3,
      "endTime": 28.9
    },
    {
      "id": "s6",
      "role": "Woman",
      "text": "I'm sorry.",
      "translation": "",
      "startTime": 28.91,
      "endTime": 30.28
    },
    {
      "id": "s7",
      "role": "Man",
      "text": "We don't have any.",
      "translation": "",
      "startTime": 30.29,
      "endTime": 32.84
    },
    {
      "id": "s8",
      "role": "Woman",
      "text": "But my sister bought this pair last month.",
      "translation": "",
      "startTime": 32.85,
      "endTime": 37.51
    },
    {
      "id": "s9",
      "role": "Man",
      "text": "Did she buy them here?",
      "translation": "",
      "startTime": 37.52,
      "endTime": 39.93
    },
    {
      "id": "s10",
      "role": "Woman",
      "text": "No, she bought them in the U. S.",
      "translation": "",
      "startTime": 39.94,
      "endTime": 44.35
    },
    {
      "id": "s11",
      "role": "Man",
      "text": "We had some shoes like those a month ago, but we don't have any now.",
      "translation": "",
      "startTime": 44.36,
      "endTime": 51.81
    },
    {
      "id": "s12",
      "role": "Woman",
      "text": "Can you get a pair for me, please?",
      "translation": "",
      "startTime": 51.82,
      "endTime": 55.51
    },
    {
      "id": "s13",
      "role": "Man",
      "text": "I'm afraid that I can't.",
      "translation": "",
      "startTime": 55.52,
      "endTime": 58.53
    },
    {
      "id": "s14",
      "role": "Woman",
      "text": "They were in fashion last year and the year before last.",
      "translation": "",
      "startTime": 58.54,
      "endTime": 64.19
    },
    {
      "id": "s15",
      "role": "Man",
      "text": "But they're not in fashion this year.",
      "translation": "",
      "startTime": 64.2,
      "endTime": 69.09
    },
    {
      "id": "s16",
      "role": "Woman",
      "text": "These shoes are in fashion now.",
      "translation": "",
      "startTime": 69.1,
      "endTime": 73.08
    },
    {
      "id": "s17",
      "role": "Man",
      "text": "They look very uncomfortable.",
      "translation": "",
      "startTime": 73.09,
      "endTime": 76.45
    },
    {
      "id": "s18",
      "role": "Woman",
      "text": "They are very uncomfortable.",
      "translation": "",
      "startTime": 76.46,
      "endTime": 79.47
    },
    {
      "id": "s19",
      "role": "Man",
      "text": "But women always wear uncomfortable shoes!",
      "translation": "",
      "startTime": 79.48,
      "endTime": 84.48
    }
  ]
}