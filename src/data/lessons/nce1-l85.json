    },
    {
      "id": "intro_2",
      "role": "Narrator",
      "text": "'Paris in the Spring'.",
      "translation": "",
      "startTime": 2.94,
      "endTime": 6.33
    },
    {
      "id": "intro_3",
      "role": "Narrator",
      "text": "Listen to the tape then answer this question.",
      "translation": "",
      "startTime": 6.34,
      "endTime": 11.74
    },
    {
      "id": "intro_4",
      "role": "Narrator",
      "text": "At what time of year did Ken visit Paris?",
      "translation": "",
      "startTime": 11.75,
      "endTime": 18.62
    },
    {
      "id": "s1",
      "role": "Man",
      "text": "Hello, Ken.",
      "translation": "",
      "startTime": 18.63,
      "endTime": 20.17
    },
    {
      "id": "s2",
      "role": "Woman",
      "text": "Hi, George.",
      "translation": "",
      "startTime": 20.18,
      "endTime": 22.19
    },
    {
      "id": "s3",
      "role": "Man",
      "text": "Have you just been to the cinema?",
      "translation": "",
      "startTime": 22.2,
      "endTime": 25.2
    },
    {
      "id": "s4",
      "role": "Woman",
      "text": "Yes, I have.",
      "translation": "",
      "startTime": 25.21,
      "endTime": 27.5
    },
    {
      "id": "s5",
      "role": "Narrator",
      "text": "What's on?",
      "translation": "",
      "startTime": 27.51,
      "endTime": 29.13
    },
    {
      "id": "s6",
      "role": "Man",
      "text": "'Paris in the Spring'.",
      "translation": "",
      "startTime": 29.14,
      "endTime": 31.65
    },
    {
      "id": "s7",
      "role": "Woman",
      "text": "Oh, I've already seen it.",
      "translation": "",
      "startTime": 31.66,
      "endTime": 34.86
    },
    {
      "id": "s8",
      "role": "Man",
      "text": "I saw it on television last year.",
      "translation": "",
      "startTime": 34.87,
      "endTime": 38.83
    },
    {
      "id": "s9",
      "role": "Woman",
      "text": "It's an old film, but it's very good.",
      "translation": "",
      "startTime": 38.84,
      "endTime": 43.42
    },
    {
      "id": "s10",
      "role": "Man",
      "text": "Paris is a beautiful city.",
      "translation": "",
      "startTime": 43.43,
      "endTime": 46.8
    },
    {
      "id": "s11",
      "role": "Woman",
      "text": "I've never been there.",
      "translation": "",
      "startTime": 46.81,
      "endTime": 48.96
    },
    {
      "id": "s12",
      "role": "Man",
      "text": "Have you ever been there, Ken?",
      "translation": "",
      "startTime": 48.97,
      "endTime": 51.99
    },
    {
      "id": "s13",
      "role": "Woman",
      "text": "Yes, I have.",
      "translation": "",
      "startTime": 52,
      "endTime": 53.94
    },
    {
      "id": "s14",
      "role": "Man",
      "text": "I was there in April.",
      "translation": "",
      "startTime": 53.95,
      "endTime": 57.34
    },
    {
      "id": "s15",
      "role": "Woman",
      "text": "Paris in the spring, eh?",
      "translation": "",
      "startTime": 57.35,
      "endTime": 60.45
    },
    {
      "id": "s16",
      "role": "Man",
      "text": "It was spring, but the weather was awful.",
      "translation": "",
      "startTime": 60.46,
      "endTime": 64.67
    },
    {
      "id": "s17",
      "role": "Woman",
      "text": "It rained all the time.",
      "translation": "",
      "startTime": 64.68,
      "endTime": 67.88
    },
    {
      "id": "s18",
      "role": "Man",
      "text": "Just like London!",
      "translation": "",
      "startTime": 67.89,
      "endTime": 72.89
    }
  ]
}