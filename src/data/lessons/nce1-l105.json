    {
      "id": "intro_2",
      "role": "Narrator",
      "text": "Full of mistakes",
      "translation": "",
      "startTime": 3.45,
      "endTime": 7.07
    },
    {
      "id": "intro_3",
      "role": "Narrator",
      "text": "Listen to the tape then answer this question.",
      "translation": "",
      "startTime": 7.08,
      "endTime": 13.48
    },
    {
      "id": "intro_4",
      "role": "Narrator",
      "text": "What was Sandra's present?",
      "translation": "",
      "startTime": 13.49,
      "endTime": 18.32
    },
    {
      "id": "s1",
      "role": "Man",
      "text": "Where's Sandra, Bob?",
      "translation": "",
      "startTime": 18.33,
      "endTime": 20.62
    },
    {
      "id": "s2",
      "role": "Woman",
      "text": "I want her.",
      "translation": "",
      "startTime": 20.63,
      "endTime": 23.4
    },
    {
      "id": "s3",
      "role": "Man",
      "text": "Do you want to speak to her?",
      "translation": "",
      "startTime": 23.41,
      "endTime": 27.37
    },
    {
      "id": "s4",
      "role": "Woman",
      "text": "Yes, I do.",
      "translation": "",
      "startTime": 27.38,
      "endTime": 29.61
    },
    {
      "id": "s5",
      "role": "Man",
      "text": "I want her to come to my office.",
      "translation": "",
      "startTime": 29.62,
      "endTime": 33.41
    },
    {
      "id": "s6",
      "role": "Woman",
      "text": "Tell her to come at once.",
      "translation": "",
      "startTime": 33.42,
      "endTime": 37.21
    },
    {
      "id": "s7",
      "role": "Man",
      "text": "Did you want to see me?",
      "translation": "",
      "startTime": 37.22,
      "endTime": 40.64
    },
    {
      "id": "s8",
      "role": "Woman",
      "text": "Ah, yes, Sandra.",
      "translation": "",
      "startTime": 40.65,
      "endTime": 43.7
    },
    {
      "id": "s9",
      "role": "Narrator",
      "text": "How do you spell 'intelligent'?",
      "translation": "",
      "startTime": 43.71,
      "endTime": 47.25
    },
    {
      "id": "s10",
      "role": "Man",
      "text": "Can you tell me?",
      "translation": "",
      "startTime": 47.26,
      "endTime": 49.6
    },
    {
      "id": "s11",
      "role": "Woman",
      "text": "I-N-T-E-L-L-I-G-E-N-T.",
      "translation": "",
      "startTime": 49.61,
      "endTime": 60.82
    },
    {
      "id": "s12",
      "role": "Man",
      "text": "That's right.",
      "translation": "",
      "startTime": 60.83,
      "endTime": 62.78
    },
    {
      "id": "s13",
      "role": "Woman",
      "text": "You've typed it with only one 'L'.",
      "translation": "",
      "startTime": 62.79,
      "endTime": 66.74
    },
    {
      "id": "s14",
      "role": "Man",
      "text": "This letter's full of mistakes.",
      "translation": "",
      "startTime": 66.75,
      "endTime": 70.64
    },
    {
      "id": "s15",
      "role": "Woman",
      "text": "I want you to type it again.",
      "translation": "",
      "startTime": 70.65,
      "endTime": 74.88
    },
    {
      "id": "s16",
      "role": "Man",
      "text": "Yes, I'll do that.",
      "translation": "",
      "startTime": 74.89,
      "endTime": 77.6
    },
    {
      "id": "s17",
      "role": "Woman",
      "text": "I'm sorry about that.",
      "translation": "",
      "startTime": 77.61,
      "endTime": 80.42
    },
    {
      "id": "s18",
      "role": "Man",
      "text": "And here's a little present for you.",
      "translation": "",
      "startTime": 80.43,
      "endTime": 83.77
    },
    {
      "id": "s19",
      "role": "Narrator",
      "text": "What is it?",
      "translation": "",
      "startTime": 83.78,
      "endTime": 85.47
    },
    {
      "id": "s20",
      "role": "Man",
      "text": "It's a dictionary.",
      "translation": "",
      "startTime": 85.48,
      "endTime": 87.6
    },
    {
      "id": "s21",
      "role": "Woman",
      "text": "I hope it'll help you.",
      "translation": "",
      "startTime": 87.61,
      "endTime": 92.61
    }
  ]
}