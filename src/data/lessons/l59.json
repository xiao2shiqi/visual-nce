    {
      "id": "intro_2",
      "role": "Narrator",
      "text": "Is that all?",
      "translation": "",
      "startTime": 3.2,
      "endTime": 6.17
    },
    {
      "id": "intro_3",
      "role": "Narrator",
      "text": "Listen to the tape then answer this question.",
      "translation": "",
      "startTime": 6.18,
      "endTime": 11.76
    },
    {
      "id": "intro_4",
      "role": "Narrator",
      "text": "Does the lady buy any chalk?",
      "translation": "",
      "startTime": 11.77,
      "endTime": 16.32
    },
    {
      "id": "s1",
      "role": "Man",
      "text": "I want some envelopes, please.",
      "translation": "",
      "startTime": 16.33,
      "endTime": 20.24
    },
    {
      "id": "s2",
      "role": "Woman",
      "text": "Do you want the large size or the small size?",
      "translation": "",
      "startTime": 20.25,
      "endTime": 25.2
    },
    {
      "id": "s3",
      "role": "Man",
      "text": "The large size, please.",
      "translation": "",
      "startTime": 25.21,
      "endTime": 28.54
    },
    {
      "id": "s4",
      "role": "Woman",
      "text": "Do you have any writing paper?",
      "translation": "",
      "startTime": 28.55,
      "endTime": 31.83
    },
    {
      "id": "s5",
      "role": "Man",
      "text": "Yes, we do.",
      "translation": "",
      "startTime": 31.84,
      "endTime": 34.4
    },
    {
      "id": "s6",
      "role": "Woman",
      "text": "I don't have any small pads.",
      "translation": "",
      "startTime": 34.41,
      "endTime": 38.33
    },
    {
      "id": "s7",
      "role": "Man",
      "text": "I only have large ones.",
      "translation": "",
      "startTime": 38.34,
      "endTime": 41.65
    },
    {
      "id": "s8",
      "role": "Woman",
      "text": "Do you want a pad?",
      "translation": "",
      "startTime": 41.66,
      "endTime": 44.16
    },
    {
      "id": "s9",
      "role": "Man",
      "text": "Yes, please.",
      "translation": "",
      "startTime": 44.17,
      "endTime": 46.43
    },
    {
      "id": "s10",
      "role": "Woman",
      "text": "And I want some glue.",
      "translation": "",
      "startTime": 46.44,
      "endTime": 49.86
    },
    {
      "id": "s11",
      "role": "Man",
      "text": "A bottle of glue.",
      "translation": "",
      "startTime": 49.87,
      "endTime": 53.05
    },
    {
      "id": "s12",
      "role": "Woman",
      "text": "And I want a large box of chalk, too.",
      "translation": "",
      "startTime": 53.06,
      "endTime": 58.2
    },
    {
      "id": "s13",
      "role": "Man",
      "text": "I only have small boxes.",
      "translation": "",
      "startTime": 58.21,
      "endTime": 61.45
    },
    {
      "id": "s14",
      "role": "Woman",
      "text": "Do you want one?",
      "translation": "",
      "startTime": 61.46,
      "endTime": 63.07
    },
    {
      "id": "s15",
      "role": "Man",
      "text": "No, thank you.",
      "translation": "",
      "startTime": 63.08,
      "endTime": 64.93
    },
    {
      "id": "s16",
      "role": "Woman",
      "text": "Is that all?",
      "translation": "",
      "startTime": 64.94,
      "endTime": 66.56
    },
    {
      "id": "s17",
      "role": "Man",
      "text": "That's all, thank you.",
      "translation": "",
      "startTime": 66.57,
      "endTime": 69.28
    },
    {
      "id": "s18",
      "role": "Narrator",
      "text": "What else do you want?",
      "translation": "",
      "startTime": 69.29,
      "endTime": 71.54
    },
    {
      "id": "s19",
      "role": "Man",
      "text": "I want my change.",
      "translation": "",
      "startTime": 71.55,
      "endTime": 76.55
    }
  ]
}