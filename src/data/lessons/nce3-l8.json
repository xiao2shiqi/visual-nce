    {
      "id": "intro_4",
      "role": "Narrator",
      "text": "The Great St.Bernard Pass connects Switzerland to Italy.",
      "translation": "",
      "startTime": 15.03,
      "endTime": 20.48
    },
    {
      "id": "s1",
      "role": "Man",
      "text": "At 2,473 metres, it is the highest mountain pass in Europe.",
      "translation": "",
      "startTime": 20.49,
      "endTime": 28.14,
      "analysis": {
        "grammar": [
          {
            "text": "it",
            "label": "人称代词/主语"
          },
          {
