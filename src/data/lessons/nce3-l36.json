  "segments": [
    {
      "id": "intro_1",
      "role": "Narrator",
      "text": "Lesson 36",
      "translation": "",
      "startTime": 0.13,
      "endTime": 2.11,
      "analysis": {
        "grammar": [],
        "words": [
          {
            "word": "Lesson",
