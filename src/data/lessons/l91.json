  "segments": [
    {
      "id": "intro_1",
      "role": "Narrator",
      "text": "Lesson 91",
      "translation": "",
      "startTime": 0.91,
      "endTime": 3.44
    },
    {
      "id": "intro_2",
      "role": "Narrator",
      "text": "Poor Ian!",
      "translation": "",
      "startTime": 3.45,
      "endTime": 6.25
    },
    {
      "id": "intro_3",
      "role": "Narrator",
      "text": "Listen to the tape then answer this question.",
      "translation": "",
      "startTime": 6.26,
      "endTime": 12.12
    },
    {
      "id": "intro_4",
      "role": "Narrator",
      "text": "Who wanted to sell the house?",
      "translation": "",
      "startTime": 12.13,
      "endTime": 17.54
    },
    {
      "id": "s1",
      "role": "Man",
      "text": "Has Ian sold his house yet?",
      "translation": "",
      "startTime": 17.55,
      "endTime": 21.24
    },
    {
      "id": "s2",
      "role": "Woman",
      "text": "Yes, he has.",
      "translation": "",
      "startTime": 21.25,
      "endTime": 23.2
    },
    {
      "id": "s3",
      "role": "Man",
      "text": "He sold it last week.",
      "translation": "",
      "startTime": 23.21,
      "endTime": 26.91
    },
    {
      "id": "s4",
      "role": "Woman",
      "text": "Has he moved to his new house yet?",
      "translation": "",
      "startTime": 26.92,
      "endTime": 31.44
    },
    {
      "id": "s5",
      "role": "Man",
      "text": "No, not yet.",
      "translation": "",
      "startTime": 31.45,
      "endTime": 33.8
    },
    {
      "id": "s6",
      "role": "Woman",
      "text": "He's still here.",
      "translation": "",
      "startTime": 33.81,
      "endTime": 36.04
    },
    {
      "id": "s7",
      "role": "Man",
      "text": "He's going to move tomorrow.",
      "translation": "",
      "startTime": 36.05,
      "endTime": 39.81
    },
    {
      "id": "s8",
      "role": "Woman",
      "text": "When? Tomorrow morning?",
      "translation": "",
      "startTime": 39.82,
      "endTime": 44.11
    },
    {
      "id": "s9",
      "role": "Man",
      "text": "No. Tomorrow afternoon.",
      "translation": "",
      "startTime": 44.12,
      "endTime": 47.82
    },
    {
      "id": "s10",
      "role": "Woman",
      "text": "I'll miss him.",
      "translation": "",
      "startTime": 47.83,
      "endTime": 49.88
    },
    {
      "id": "s11",
      "role": "Man",
      "text": "He has always been a good neighbour.",
      "translation": "",
      "startTime": 49.89,
      "endTime": 54.56
    },
    {
      "id": "s12",
      "role": "Woman",
      "text": "He's a very nice person.",
      "translation": "",
      "startTime": 54.57,
      "endTime": 58.16
    },
    {
      "id": "s13",
      "role": "Man",
      "text": "We'll all miss him.",
      "translation": "",
      "startTime": 58.17,
      "endTime": 62.2
    },
    {
      "id": "s14",
      "role": "Woman",
      "text": "When will the new people move into this house?",
      "translation": "",
      "startTime": 62.21,
      "endTime": 67.53
    },
    {
      "id": "s15",
      "role": "Man",
      "text": "I think that they'll move in the day after tomorrow.",
      "translation": "",
      "startTime": 67.54,
      "endTime": 74.21
    },
    {
      "id": "s16",
      "role": "Woman",
      "text": "Will you see Ian today, Jenny?",
      "translation": "",
      "startTime": 74.22,
      "endTime": 79.7
    },
    {
      "id": "s17",
      "role": "Man",
      "text": "Yes, I will.",
      "translation": "",
      "startTime": 79.71,
      "endTime": 83.08
    },
    {
      "id": "s18",
      "role": "Woman",
      "text": "Please give him my regards.",
      "translation": "",
      "startTime": 83.09,
      "endTime": 87.82
    },
    {
      "id": "s19",
      "role": "Man",
      "text": "Poor Ian! He didn't want to leave this house.",
      "translation": "",
      "startTime": 87.83,
      "endTime": 94.48
    },
    {
      "id": "s20",
      "role": "Woman",
      "text": "No, he didn't want to leave, but his wife did!",
      "translation": "",
      "startTime": 94.49,
      "endTime": 99.49
    }
  ]
}