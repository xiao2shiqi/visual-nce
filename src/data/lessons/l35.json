    },
    {
      "id": "intro_4",
      "role": "Narrator",
      "text": "Are the children coming out of the park or going into it?",
      "translation": "",
      "startTime": 11.74,
      "endTime": 18.96
    },
    {
      "id": "s1",
      "role": "Man",
      "text": "This is a photograph of our village.",
      "translation": "",
      "startTime": 18.97,
      "endTime": 23.49
    },
    {
      "id": "s2",
      "role": "Woman",
      "text": "Our village is in a valley.",
      "translation": "",
      "startTime": 23.5,
      "endTime": 26.88
    },
    {
      "id": "s3",
      "role": "Man",
      "text": "It is between two hills.",
      "translation": "",
      "startTime": 26.89,
      "endTime": 30.23
    },
    {
      "id": "s4",
      "role": "Woman",
      "text": "The village is on a river.",
      "translation": "",
      "startTime": 30.24,
      "endTime": 34.68
    },
    {
      "id": "s5",
      "role": "Man",
      "text": "Here is another photograph of the village.",
      "translation": "",
      "startTime": 34.69,
      "endTime": 39.61
    },
    {
      "id": "s6",
      "role": "Woman",
      "text": "My wife and I are walking along the banks of the river.",
      "translation": "",
      "startTime": 39.62,
      "endTime": 46.38
    },
    {
      "id": "s7",
      "role": "Man",
      "text": "We are on the left.",
      "translation": "",
      "startTime": 46.39,
      "endTime": 49.58
    },
    {
      "id": "s8",
      "role": "Woman",
      "text": "There is a boy in the water.",
      "translation": "",
      "startTime": 49.59,
      "endTime": 52.92
    },
    {
      "id": "s9",
      "role": "Man",
      "text": "He is swimming across the river.",
      "translation": "",
      "startTime": 52.93,
      "endTime": 58.25
    },
    {
      "id": "s10",
      "role": "Woman",
      "text": "Here is another photograph.",
      "translation": "",
      "startTime": 58.26,
      "endTime": 61.75
    },
    {
      "id": "s11",
      "role": "Man",
      "text": "This is the school building.",
      "translation": "",
      "startTime": 61.76,
      "endTime": 64.75
    },
    {
      "id": "s12",
      "role": "Woman",
      "text": "It is beside a park.",
      "translation": "",
      "startTime": 64.76,
      "endTime": 68.08
    },
    {
      "id": "s13",
      "role": "Man",
      "text": "The park is on the right.",
      "translation": "",
      "startTime": 68.09,
      "endTime": 71.51
    },
    {
      "id": "s14",
      "role": "Woman",
      "text": "Some children are coming out of the building.",
      "translation": "",
      "startTime": 71.52,
      "endTime": 75.98
    },
    {
      "id": "s15",
      "role": "Man",
      "text": "Some of them are going into the park.",
      "translation": "",
      "startTime": 75.99,
      "endTime": 80.99
    }
  ]
