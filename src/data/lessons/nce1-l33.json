      "translation": "",
      "startTime": 3.26,
      "endTime": 6.01
    },
    {
      "id": "intro_3",
      "role": "Narrator",
      "text": "Listen to the tape then answer this question.",
      "translation": "",
      "startTime": 6.02,
      "endTime": 11.41
    },
    {
      "id": "intro_4",
      "role": "Narrator",
      "text": "Where is the Jones family?",
      "translation": "",
      "startTime": 11.42,
      "endTime": 16.25
    },
    {
      "id": "s1",
      "role": "Man",
      "text": "It is a fine day today.",
      "translation": "",
      "startTime": 16.26,
      "endTime": 19.96
    },
    {
      "id": "s2",
      "role": "Woman",
      "text": "There are some clouds in the sky,",
      "translation": "",
      "startTime": 19.97,
      "endTime": 24.17
    },
    {
      "id": "s3",
      "role": "Man",
      "text": "but the sun is shining.",
      "translation": "",
      "startTime": 24.18,
      "endTime": 27.73
    },
    {
      "id": "s4",
      "role": "Woman",
      "text": "Mr. Jones is with his family.",
      "translation": "",
      "startTime": 27.74,
      "endTime": 31.77
    },
    {
      "id": "s5",
      "role": "Man",
      "text": "They are walking over the bridge.",
      "translation": "",
      "startTime": 31.78,
      "endTime": 35.82
    },
    {
      "id": "s6",
      "role": "Woman",
      "text": "There are some boats on the river.",
      "translation": "",
      "startTime": 35.83,
      "endTime": 39.85
    },
    {
      "id": "s7",
      "role": "Man",
      "text": "Mr. Jones and his wife are looking at them.",
      "translation": "",
      "startTime": 39.86,
      "endTime": 45.72
    },
    {
      "id": "s8",
      "role": "Woman",
      "text": "Sally is looking at a big ship.",
      "translation": "",
      "startTime": 45.73,
      "endTime": 50.17
    },
    {
      "id": "s9",
      "role": "Man",
      "text": "The ship is going under the bridge.",
      "translation": "",
      "startTime": 50.18,
      "endTime": 54.74
    },
    {
      "id": "s10",
      "role": "Woman",
      "text": "Tim is looking at an aeroplane.",
      "translation": "",
      "startTime": 54.75,
      "endTime": 59.5
    },
    {
      "id": "s11",
      "role": "Man",
      "text": "The aeroplane is flying over the river.",
      "translation": "",
      "startTime": 59.51,
      "endTime": 64.51
    }
  ]
