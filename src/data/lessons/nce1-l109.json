  "segments": [
    {
      "id": "intro_1",
      "role": "Narrator",
      "text": "Lesson 109",
      "translation": "",
      "startTime": 0.67,
      "endTime": 3.26
    },
    {
      "id": "intro_2",
      "role": "Narrator",
      "text": "A good idea",
      "translation": "",
      "startTime": 3.27,
      "endTime": 6.15
    },
    {
      "id": "intro_3",
      "role": "Narrator",
      "text": "Listen to the tape then answer this question.",
      "translation": "",
      "startTime": 6.16,
      "endTime": 11.66
    },
    {
      "id": "intro_4",
      "role": "Narrator",
      "text": "What does Jane have with her coffee?",
      "translation": "",
      "startTime": 11.67,
      "endTime": 16.96
    },
    {
      "id": "s1",
      "role": "Man",
      "text": "Shall I make some coffee, Jane?",
      "translation": "",
      "startTime": 16.97,
      "endTime": 21.75
    },
    {
      "id": "s2",
      "role": "Woman",
      "text": "That's a good idea, Charlotte.",
      "translation": "",
      "startTime": 21.76,
      "endTime": 26.06
    },
    {
      "id": "s3",
      "role": "Man",
      "text": "It's ready.",
      "translation": "",
      "startTime": 26.07,
      "endTime": 27.96
    },
    {
      "id": "s4",
      "role": "Woman",
      "text": "Do you want any milk?",
      "translation": "",
      "startTime": 27.97,
      "endTime": 30.93
    },
    {
      "id": "s5",
      "role": "Man",
      "text": "Just a little, please.",
      "translation": "",
      "startTime": 30.94,
      "endTime": 34.42
    },
    {
      "id": "s6",
      "role": "Narrator",
      "text": "What about some sugar?",
      "translation": "",
      "startTime": 34.43,
      "endTime": 37.45
    },
    {
      "id": "s7",
      "role": "Man",
      "text": "Two teaspoonfuls?",
      "translation": "",
      "startTime": 37.46,
      "endTime": 40.67
    },
    {
      "id": "s8",
      "role": "Woman",
      "text": "No, less than that.",
      "translation": "",
      "startTime": 40.68,
      "endTime": 43.62
    },
    {
      "id": "s9",
      "role": "Man",
      "text": "One and a half teaspoonfuls, please.",
      "translation": "",
      "startTime": 43.63,
      "endTime": 48.28
    },
    {
      "id": "s10",
      "role": "Woman",
      "text": "That's enough for me.",
      "translation": "",
      "startTime": 48.29,
      "endTime": 52.02
    },
    {
      "id": "s11",
      "role": "Man",
      "text": "That was very nice.",
      "translation": "",
      "startTime": 52.03,
      "endTime": 55.24
    },
    {
      "id": "s12",
      "role": "Woman",
      "text": "Would you like some more?",
      "translation": "",
      "startTime": 55.25,
      "endTime": 58.04
    },
    {
      "id": "s13",
      "role": "Man",
      "text": "Yes, please.",
      "translation": "",
      "startTime": 58.05,
      "endTime": 61.39
    },
    {
      "id": "s14",
      "role": "Woman",
      "text": "I'd like a cigarette, too.",
      "translation": "",
      "startTime": 61.4,
      "endTime": 64.84
    },
    {
      "id": "s15",
      "role": "Man",
      "text": "May I have one?",
      "translation": "",
      "startTime": 64.85,
      "endTime": 67.49
    },
    {
      "id": "s16",
      "role": "Woman",
      "text": "Of course.",
      "translation": "",
      "startTime": 67.5,
      "endTime": 69.45
    },
    {
      "id": "s17",
      "role": "Man",
      "text": "I think there are a few in that box.",
      "translation": "",
      "startTime": 69.46,
      "endTime": 74.66
    },
    {
      "id": "s18",
      "role": "Woman",
      "text": "I'm afraid it's empty.",
      "translation": "",
      "startTime": 74.67,
      "endTime": 78.17
    },
    {
      "id": "s19",
      "role": "Narrator",
      "text": "What a pity!",
      "translation": "",
      "startTime": 78.18,
      "endTime": 80.49
    },
    {
      "id": "s20",
      "role": "Man",
      "text": "It doesn't matter.",
      "translation": "",
      "startTime": 80.5,
      "endTime": 83.22
    },
    {
      "id": "s21",
      "role": "Woman",
      "text": "Have a biscuit instead.",
      "translation": "",
      "startTime": 83.23,
      "endTime": 86.21
    },
    {
      "id": "s22",
      "role": "Man",
      "text": "Eat more and smoke less!",
      "translation": "",
      "startTime": 86.22,
      "endTime": 90.31
    },
    {
      "id": "s23",
      "role": "Woman",
      "text": "That's very good advice!",
      "translation": "",
      "startTime": 90.32,
      "endTime": 95.32
    }
  ]
}