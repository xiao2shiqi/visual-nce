    },
    {
      "id": "s4",
      "role": "Woman",
      "text": "She was sentimentally attached to this house,",
      "translation": "",
      "startTime": 40.36,
      "endTime": 43.81,
      "analysis": {
        "grammar": [
          {
            "text": "this",
            "label": "指示代词"
