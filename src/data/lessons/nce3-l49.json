      "role": "Woman",
      "text": "had had a difficult time trying to conceal their amusement.",
      "translation": "",
      "startTime": 233.35,
      "endTime": 238.03,
      "analysis": {
        "grammar": [
          {
            "text": "their",
            "label": "形容词性物主代词"
          }
        ],
        "words": []
      }
