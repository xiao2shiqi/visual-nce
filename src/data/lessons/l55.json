    {
      "id": "intro_2",
      "role": "Narrator",
      "text": "The Sawyer family",
      "translation": "",
      "startTime": 3.26,
      "endTime": 6.22
    },
    {
      "id": "intro_3",
      "role": "Narrator",
      "text": "Listen to the tape then answer this question.",
      "translation": "",
      "startTime": 6.23,
      "endTime": 11.82
    },
    {
      "id": "intro_4",
      "role": "Narrator",
      "text": "When do the children do their homework?",
      "translation": "",
      "startTime": 11.83,
      "endTime": 16.8
    },
    {
      "id": "s1",
      "role": "Man",
      "text": "The Sawyers live at 87 King Street.",
      "translation": "",
      "startTime": 16.81,
      "endTime": 21.93
    },
    {
      "id": "s2",
      "role": "Woman",
      "text": "In the morning, Mr. Sawyer goes to work and the children go to school.",
      "translation": "",
      "startTime": 21.94,
      "endTime": 29.96
    },
    {
      "id": "s3",
      "role": "Man",
      "text": "Their father takes them to school every day.",
      "translation": "",
      "startTime": 29.97,
      "endTime": 35.1
    },
    {
      "id": "s4",
      "role": "Woman",
      "text": "Mrs. Sawyer stays at home every day.",
      "translation": "",
      "startTime": 35.11,
      "endTime": 39.2
    },
    {
      "id": "s5",
      "role": "Man",
      "text": "She does the housework.",
      "translation": "",
      "startTime": 39.21,
      "endTime": 42.06
    },
    {
      "id": "s6",
      "role": "Woman",
      "text": "She always eats her lunch at noon.",
      "translation": "",
      "startTime": 42.07,
      "endTime": 46.81
    },
    {
      "id": "s7",
      "role": "Man",
      "text": "In the afternoon, she usually sees her friends.",
      "translation": "",
      "startTime": 46.82,
      "endTime": 52.1
    },
    {
      "id": "s8",
      "role": "Woman",
      "text": "They often drink tea together.",
      "translation": "",
      "startTime": 52.11,
      "endTime": 55.85
    },
    {
      "id": "s9",
      "role": "Man",
      "text": "In the evening, the children come home from school.",
      "translation": "",
      "startTime": 55.86,
      "endTime": 60.78
    },
    {
      "id": "s10",
      "role": "Woman",
      "text": "They arrive home early.",
      "translation": "",
      "startTime": 60.79,
      "endTime": 64.19
    },
    {
      "id": "s11",
      "role": "Man",
      "text": "Mr. Sawyer comes home from work.",
      "translation": "",
      "startTime": 64.2,
      "endTime": 67.94
    },
    {
      "id": "s12",
      "role": "Woman",
      "text": "He arrives home late.",
      "translation": "",
      "startTime": 67.95,
      "endTime": 71.48
    },
    {
      "id": "s13",
      "role": "Man",
      "text": "At night, the children always do their homework.",
      "translation": "",
      "startTime": 71.49,
      "endTime": 76.22
    },
    {
      "id": "s14",
      "role": "Woman",
      "text": "Then they go to bed.",
      "translation": "",
      "startTime": 76.23,
      "endTime": 79.16
    },
    {
      "id": "s15",
      "role": "Man",
      "text": "Mr. Sawyer usually reads his newspaper, but sometimes he and his wife watch television.",
      "translation": "",
      "startTime": 79.17,
      "endTime": 84.17
    }
  ]
}