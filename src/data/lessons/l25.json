      "startTime": 39.62,
      "endTime": 42.94
    },
    {
      "id": "s7",
      "role": "Man",
      "text": "It is on the left.",
      "translation": "",
      "startTime": 42.95,
      "endTime": 46.21
    },
    {
      "id": "s8",
      "role": "Woman",
      "text": "There is a table in the middle of the room.",
      "translation": "",
      "startTime": 46.22,
      "endTime": 51.53
    },
    {
      "id": "s9",
      "role": "Man",
      "text": "There is a bottle on the table.",
      "translation": "",
      "startTime": 51.54,
      "endTime": 55.26
    },
    {
      "id": "s10",
      "role": "Woman",
      "text": "The bottle is empty.",
