      "startTime": 12.58,
      "endTime": 18.3
    },
    {
      "id": "s1",
      "role": "Man",
      "text": "Mrs. Smith's kitchen is small.",
      "translation": "",
      "startTime": 18.31,
      "endTime": 23.11
    },
    {
      "id": "s2",
      "role": "Woman",
      "text": "There is a refrigerator in the kitchen.",
      "translation": "",
      "startTime": 23.12,
      "endTime": 27.7
    },
    {
      "id": "s3",
      "role": "Man",
      "text": "The refrigerator is white.",
      "translation": "",
      "startTime": 27.71,
      "endTime": 31.61
    },
    {
      "id": "s4",
      "role": "Woman",
      "text": "It is on the right.",
      "translation": "",
      "startTime": 31.62,
      "endTime": 35.02
    },
    {
      "id": "s5",
      "role": "Man",
      "text": "There is an electric cooker in the kitchen.",
      "translation": "",
      "startTime": 35.03,
      "endTime": 39.61
    },
    {
      "id": "s6",
      "role": "Woman",
      "text": "The cooker is blue.",
