    },
    {
      "id": "intro_2",
      "role": "Narrator",
      "text": "He's awful!",
      "translation": "",
      "startTime": 2.95,
      "endTime": 5.83
    },
    {
      "id": "intro_3",
      "role": "Narrator",
      "text": "Listen to the tape then answer this question.",
      "translation": "",
      "startTime": 5.84,
      "endTime": 11.81
    },
    {
      "id": "intro_4",
      "role": "Narrator",
      "text": "How did Pauline answer the telephone at nine o'clock?",
      "translation": "",
      "startTime": 11.82,
      "endTime": 19.63
    },
    {
      "id": "s1",
      "role": "Narrator",
      "text": "What's Ron Marston like, Pauline?",
      "translation": "",
      "startTime": 19.64,
      "endTime": 24.06
    },
    {
      "id": "s2",
      "role": "Man",
      "text": "He's awful!",
      "translation": "",
      "startTime": 24.07,
      "endTime": 26.61
    },
    {
      "id": "s3",
      "role": "Woman",
      "text": "He telephoned me four times yesterday and three times the day before yesterday.",
      "translation": "",
      "startTime": 26.62,
      "endTime": 37.21
    },
    {
      "id": "s4",
      "role": "Man",
      "text": "He telephoned the office yesterday morning and yesterday afternoon.",
      "translation": "",
      "startTime": 37.22,
      "endTime": 45.54
    },
    {
      "id": "s5",
      "role": "Woman",
      "text": "My boss answered the telephone.",
      "translation": "",
      "startTime": 45.55,
      "endTime": 50.13
    },
    {
      "id": "s6",
      "role": "Narrator",
      "text": "What did your boss say to him?",
      "translation": "",
      "startTime": 50.14,
      "endTime": 53.79
    },
    {
      "id": "s7",
      "role": "Man",
      "text": "He said, 'Pauline is typing letters.",
      "translation": "",
      "startTime": 53.8,
      "endTime": 58.49
    },
    {
      "id": "s8",
      "role": "Woman",
      "text": "She can't speak to you now!'",
      "translation": "",
      "startTime": 58.5,
      "endTime": 62.35
    },
    {
      "id": "s9",
      "role": "Man",
      "text": "Then I arrived home at six o'clock yesterday evening.",
      "translation": "",
      "startTime": 62.36,
      "endTime": 68.81
    },
    {
      "id": "s10",
      "role": "Woman",
      "text": "He telephoned again.",
      "translation": "",
      "startTime": 68.82,
      "endTime": 71.6
    },
    {
      "id": "s11",
      "role": "Man",
      "text": "But I didn't answer the phone!",
      "translation": "",
      "startTime": 71.61,
      "endTime": 75.66
    },
    {
      "id": "s12",
      "role": "Woman",
      "text": "Did he telephone again last night?",
      "translation": "",
      "startTime": 75.67,
      "endTime": 79.82
    },
    {
      "id": "s13",
      "role": "Man",
      "text": "Yes, he did.",
      "translation": "",
      "startTime": 79.83,
      "endTime": 82.2
    },
    {
      "id": "s14",
      "role": "Woman",
      "text": "He telephoned at nine o'clock.",
      "translation": "",
      "startTime": 82.21,
      "endTime": 86.4
    },
    {
      "id": "s15",
      "role": "Narrator",
      "text": "What did you say to him?",
      "translation": "",
      "startTime": 86.41,
      "endTime": 89.66
    },
    {
      "id": "s16",
      "role": "Man",
      "text": "I said, 'This is Pauline's mother.",
      "translation": "",
      "startTime": 89.67,
      "endTime": 93.81
    },
    {
      "id": "s17",
      "role": "Woman",
      "text": "Please don't telephone my daughter again!'",
      "translation": "",
      "startTime": 93.82,
      "endTime": 99.43
    },
    {
      "id": "s18",
      "role": "Man",
      "text": "Did he telephone again?",
      "translation": "",
      "startTime": 99.44,
      "endTime": 102.09
    },
    {
      "id": "s19",
      "role": "Woman",
      "text": "No, he didn't!",
      "translation": "",
      "startTime": 102.1,
      "endTime": 107.1
    }
  ]
}