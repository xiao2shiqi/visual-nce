    },
    {
      "id": "intro_2",
      "role": "Narrator",
      "text": "People will do anything to see a free show--even if it is a bad one.",
      "translation": "",
      "startTime": 16.68,
      "endTime": 24.42,
      "analysis": {
        "words": [
          {
