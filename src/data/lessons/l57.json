    {
      "id": "intro_2",
      "role": "Narrator",
      "text": "An unusual day",
      "translation": "",
      "startTime": 3.12,
      "endTime": 6.58
    },
    {
      "id": "intro_3",
      "role": "Narrator",
      "text": "Listen to the tape then answer this question.",
      "translation": "",
      "startTime": 6.59,
      "endTime": 12.24
    },
    {
      "id": "intro_4",
      "role": "Narrator",
      "text": "What is Mr. Sawyer doing tonight?",
      "translation": "",
      "startTime": 12.25,
      "endTime": 17.61
    },
    {
      "id": "s1",
      "role": "Man",
      "text": "It is eight o'clock.",
      "translation": "",
      "startTime": 17.62,
      "endTime": 20.43
    },
    {
      "id": "s2",
      "role": "Woman",
      "text": "The children go to school by car every day,",
      "translation": "",
      "startTime": 20.44,
      "endTime": 25.19
    },
    {
      "id": "s3",
      "role": "Man",
      "text": "but today, they are going to school on foot.",
      "translation": "",
      "startTime": 25.2,
      "endTime": 31.16
    },
    {
      "id": "s4",
      "role": "Woman",
      "text": "It is ten o'clock.",
      "translation": "",
      "startTime": 31.17,
      "endTime": 33.56
    },
    {
      "id": "s5",
      "role": "Man",
      "text": "Mrs. Sawyer usually stays at home in the morning,",
      "translation": "",
      "startTime": 33.57,
      "endTime": 38.59
    },
    {
      "id": "s6",
      "role": "Woman",
      "text": "but this morning, she is going to the shops.",
      "translation": "",
      "startTime": 38.6,
      "endTime": 44.49
    },
    {
      "id": "s7",
      "role": "Man",
      "text": "It is four o'clock.",
      "translation": "",
      "startTime": 44.5,
      "endTime": 47.15
    },
    {
      "id": "s8",
      "role": "Woman",
      "text": "In the afternoon, Mrs. Sawyer usually drinks tea in the living room.",
      "translation": "",
      "startTime": 47.16,
      "endTime": 54.05
    },
    {
      "id": "s9",
      "role": "Man",
      "text": "But this afternoon, she is drinking tea in the garden.",
      "translation": "",
      "startTime": 54.06,
      "endTime": 61.15
    },
    {
      "id": "s10",
      "role": "Woman",
      "text": "It is six o'clock.",
      "translation": "",
      "startTime": 61.16,
      "endTime": 63.82
    },
    {
      "id": "s11",
      "role": "Man",
      "text": "In the evening, the children usually do their homework,",
      "translation": "",
      "startTime": 63.83,
      "endTime": 68.91
    },
    {
      "id": "s12",
      "role": "Woman",
      "text": "but this evening, they are not doing their homework.",
      "translation": "",
      "startTime": 68.92,
      "endTime": 74.2
    },
    {
      "id": "s13",
      "role": "Man",
      "text": "At the moment, they are playing in the garden.",
      "translation": "",
      "startTime": 74.21,
      "endTime": 79.98
    },
    {
      "id": "s14",
      "role": "Woman",
      "text": "It is nine o'clock.",
      "translation": "",
      "startTime": 79.99,
      "endTime": 82.71
    },
    {
      "id": "s15",
      "role": "Man",
      "text": "Mr. Sawyer usually reads his newspaper at night.",
      "translation": "",
      "startTime": 82.72,
      "endTime": 88.02
    },
    {
      "id": "s16",
      "role": "Woman",
      "text": "But he's not reading his newspaper tonight.",
      "translation": "",
      "startTime": 88.03,
      "endTime": 92.52
    },
    {
      "id": "s17",
      "role": "Man",
      "text": "At the moment, he's reading an interesting book.",
      "translation": "",
      "startTime": 92.53,
      "endTime": 97.53
    }
  ]
}