{
  "id": "l95",
  "title": "Lesson 95: Tickets, Please.",
  "audio": "/audio/nce1/095&096ÔºçTickets, Please..mp3",
  "image": "/images/coming-soon.png",
  "segments": [
    {
      "id": "intro_1",
      "role": "Narrator",
      "text": "Lesson 95",
      "translation": "",
      "startTime": 1.01,
      "endTime": 3.51
    },
    {
      "id": "intro_2",
      "role": "Narrator",
      "text": "Tickets, please.",
      "translation": "",
      "startTime": 3.52,
      "endTime": 6.51
    },
    {
      "id": "intro_3",
      "role": "Narrator",
      "text": "Listen to the tape then answer this question.",
      "translation": "",
      "startTime": 6.52,
      "endTime": 12.46
    },
    {
      "id": "intro_4",
      "role": "Narrator",
      "text": "Why did George and Ken miss the train?",
      "translation": "",
      "startTime": 12.47,
      "endTime": 19.38
    },
    {
      "id": "s1",
      "role": "Man",
      "text": "Two return tickets to London, please.",
      "translation": "",
      "startTime": 19.39,
      "endTime": 23.43
    },
    {
      "id": "s2",
      "role": "Narrator",
      "text": "What time will the next train leave?",
      "translation": "",
      "startTime": 23.44,
      "endTime": 27.21
    },
    {
      "id": "s3",
      "role": "Man",
      "text": "At nineteen minutes past eight.",
      "translation": "",
      "startTime": 27.22,
      "endTime": 31.51
    },
    {
      "id": "s4",
      "role": "Narrator",
      "text": "Which platform?",
      "translation": "",
      "startTime": 31.52,
      "endTime": 33.39
    },
    {
      "id": "s5",
      "role": "Man",
      "text": "Platform Two.",
      "translation": "",
      "startTime": 33.4,
      "endTime": 35.17
    },
    {
      "id": "s6",
      "role": "Woman",
      "text": "Over the bridge.",
      "translation": "",
      "startTime": 35.18,
      "endTime": 38.22
    },
    {
      "id": "s7",
      "role": "Narrator",
      "text": "What time will the next train leave?",
      "translation": "",
      "startTime": 38.23,
      "endTime": 42.15
    },
    {
      "id": "s8",
      "role": "Man",
      "text": "At eight nineteen.",
      "translation": "",
      "startTime": 42.16,
      "endTime": 44.94
    },
    {
      "id": "s9",
      "role": "Woman",
      "text": "We've got plenty of time.",
      "translation": "",
      "startTime": 44.95,
      "endTime": 48.55
    },
    {
      "id": "s10",
      "role": "Man",
      "text": "It's only three minutes to eight.",
      "translation": "",
      "startTime": 48.56,
      "endTime": 52.52
    },
    {
      "id": "s11",
      "role": "Woman",
      "text": "Let's go and have a drink.",
      "translation": "",
      "startTime": 52.53,
      "endTime": 55.52
    },
    {
      "id": "s12",
      "role": "Man",
      "text": "There's a bar next door to the station.",
      "translation": "",
      "startTime": 55.53,
      "endTime": 60.68
    },
    {
      "id": "s13",
      "role": "Woman",
      "text": "We had better go back to the station now, Ken.",
      "translation": "",
      "startTime": 60.69,
      "endTime": 65.79
    },
    {
      "id": "s14",
      "role": "Man",
      "text": "Tickets, please.",
      "translation": "",
      "startTime": 65.8,
      "endTime": 68.22
    },
    {
      "id": "s15",
      "role": "Woman",
      "text": "We want to catch the eight nineteen to London.",
      "translation": "",
      "startTime": 68.23,
      "endTime": 72.9
    },
    {
      "id": "s16",
      "role": "Man",
      "text": "You've just missed it!",
      "translation": "",
      "startTime": 72.91,
      "endTime": 75.69
    },
    {
      "id": "s17",
      "role": "Narrator",
      "text": "What!",
      "translation": "",
      "startTime": 75.7,
      "endTime": 77.01
    },
    {
      "id": "s18",
      "role": "Man",
      "text": "It's only eight fifteen.",
      "translation": "",
      "startTime": 77.02,
      "endTime": 80.18
    },
    {
      "id": "s19",
      "role": "Woman",
      "text": "I'm sorry, sir.",
      "translation": "",
      "startTime": 80.19,
      "endTime": 82.23
    },
    {
      "id": "s20",
      "role": "Man",
      "text": "That clock's ten minutes slow.",
      "translation": "",
      "startTime": 82.24,
      "endTime": 86.15
    },
    {
      "id": "s21",
      "role": "Woman",
      "text": "When's the next train?",
      "translation": "",
      "startTime": 86.16,
      "endTime": 88.76
    },
    {
      "id": "s22",
      "role": "Man",
      "text": "In five hours' time!",
      "translation": "",
      "startTime": 88.77,
      "endTime": 93.77
    }
  ]
}