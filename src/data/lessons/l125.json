  "segments": [
    {
      "id": "intro_1",
      "role": "Narrator",
      "text": "Lesson 125",
      "translation": "",
      "startTime": 0.51,
      "endTime": 3.45
    },
    {
      "id": "intro_2",
      "role": "Narrator",
      "text": "Tea for two",
      "translation": "",
      "startTime": 3.46,
      "endTime": 6.25
    },
    {
      "id": "intro_3",
      "role": "Narrator",
      "text": "Listen to the tape then answer this question.",
      "translation": "",
      "startTime": 6.26,
      "endTime": 11.21
    },
    {
      "id": "intro_4",
      "role": "Narrator",
      "text": "Does Susan have tea by herself?",
      "translation": "",
      "startTime": 11.22,
      "endTime": 16.46
    },
    {
      "id": "s1",
      "role": "Man",
      "text": "Can't you come in and have tea now, Peter?",
      "translation": "",
      "startTime": 16.47,
      "endTime": 20.96
    },
    {
      "id": "s2",
      "role": "Woman",
      "text": "Not yet.",
      "translation": "",
      "startTime": 20.97,
      "endTime": 22.37
    },
    {
      "id": "s3",
      "role": "Man",
      "text": "I must water the garden first.",
      "translation": "",
      "startTime": 22.38,
      "endTime": 25.77
    },
    {
      "id": "s4",
      "role": "Woman",
      "text": "Do you have to water it now?",
      "translation": "",
      "startTime": 25.78,
      "endTime": 29.51
    },
    {
      "id": "s5",
      "role": "Man",
      "text": "I'm afraid I must.",
      "translation": "",
      "startTime": 29.52,
      "endTime": 31.53
    },
    {
      "id": "s6",
      "role": "Woman",
      "text": "Look at it!",
      "translation": "",
      "startTime": 31.54,
      "endTime": 33.01
    },
    {
      "id": "s7",
      "role": "Man",
      "text": "It's terribly dry.",
      "translation": "",
      "startTime": 33.02,
      "endTime": 35.4
    },
    {
      "id": "s8",
      "role": "Narrator",
      "text": "What a nuisance!",
      "translation": "",
      "startTime": 35.41,
      "endTime": 37.99
    },
    {
      "id": "s9",
      "role": "Man",
      "text": "Last summer it was very dry, too.",
      "translation": "",
      "startTime": 38,
      "endTime": 41.97
    },
    {
      "id": "s10",
      "role": "Woman",
      "text": "Don't you remember?",
      "translation": "",
      "startTime": 41.98,
      "endTime": 44.15
    },
    {
      "id": "s11",
      "role": "Man",
      "text": "I had to water it every day.",
      "translation": "",
      "startTime": 44.16,
      "endTime": 47.51
    },
    {
      "id": "s12",
      "role": "Woman",
      "text": "Well, I'll have tea by myself.",
      "translation": "",
      "startTime": 47.52,
      "endTime": 52.69
    },
    {
      "id": "s13",
      "role": "Man",
      "text": "That was quick!",
      "translation": "",
      "startTime": 52.7,
      "endTime": 54.72
    },
    {
      "id": "s14",
      "role": "Woman",
      "text": "Have you finished already?",
      "translation": "",
      "startTime": 54.73,
      "endTime": 57.47
    },
    {
      "id": "s15",
      "role": "Man",
      "text": "Yes.",
      "translation": "",
      "startTime": 57.48,
      "endTime": 58.63
    },
    {
      "id": "s16",
      "role": "Woman",
      "text": "Look out of the window.",
      "translation": "",
      "startTime": 58.64,
      "endTime": 61.34
    },
    {
      "id": "s17",
      "role": "Man",
      "text": "It's raining!",
      "translation": "",
      "startTime": 61.35,
      "endTime": 63.45
    },
    {
      "id": "s18",
      "role": "Woman",
      "text": "That means you don't need to water the garden.",
      "translation": "",
      "startTime": 63.46,
      "endTime": 68.79
    },
    {
      "id": "s19",
      "role": "Man",
      "text": "That was a pleasant surprise.",
      "translation": "",
      "startTime": 68.8,
      "endTime": 72.28
    },
    {
      "id": "s20",
      "role": "Woman",
      "text": "It means I can have tea, instead.",
      "translation": "",
      "startTime": 72.29,
      "endTime": 77.29
    }
  ]
}