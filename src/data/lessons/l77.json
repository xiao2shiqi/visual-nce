      "translation": "",
      "startTime": 3.02,
      "endTime": 6.46
    },
    {
      "id": "intro_3",
      "role": "Narrator",
      "text": "Listen to the tape then answer this question.",
      "translation": "",
      "startTime": 6.47,
      "endTime": 12.13
    },
    {
      "id": "intro_4",
      "role": "Narrator",
      "text": "What time of day is it, do you think? How do you know?",
      "translation": "",
      "startTime": 12.14,
      "endTime": 21.16
    },
    {
      "id": "s1",
      "role": "Man",
      "text": "Good morning, Mr. Croft.",
      "translation": "",
      "startTime": 21.17,
      "endTime": 24.96
    },
    {
      "id": "s2",
      "role": "Woman",
      "text": "Good morning, nurse.",
      "translation": "",
      "startTime": 24.97,
      "endTime": 27.47
    },
    {
      "id": "s3",
      "role": "Man",
      "text": "I want to see the dentist, please.",
      "translation": "",
      "startTime": 27.48,
      "endTime": 31.22
    },
    {
      "id": "s4",
      "role": "Woman",
      "text": "Do you have an appointment?",
      "translation": "",
      "startTime": 31.23,
      "endTime": 34.59
    },
    {
      "id": "s5",
      "role": "Man",
      "text": "No, I don't.",
      "translation": "",
      "startTime": 34.6,
      "endTime": 36.6
    },
    {
      "id": "s6",
      "role": "Woman",
      "text": "Is it urgent?",
      "translation": "",
      "startTime": 36.61,
      "endTime": 38.69
    },
    {
      "id": "s7",
      "role": "Man",
      "text": "Yes, it is.",
      "translation": "",
      "startTime": 38.7,
      "endTime": 41.28
    },
    {
      "id": "s8",
      "role": "Woman",
      "text": "It's very urgent.",
      "translation": "",
      "startTime": 41.29,
      "endTime": 44.44
    },
    {
      "id": "s9",
      "role": "Man",
      "text": "I feel awful.",
      "translation": "",
      "startTime": 44.45,
      "endTime": 46.85
    },
    {
      "id": "s10",
      "role": "Woman",
      "text": "I have a terrible toothache.",
      "translation": "",
      "startTime": 46.86,
      "endTime": 50.89
    },
    {
      "id": "s11",
      "role": "Man",
      "text": "Can you come at 10 a.m. on Monday, April 24th?",
      "translation": "",
      "startTime": 50.9,
      "endTime": 57.49
    },
    {
      "id": "s12",
      "role": "Woman",
      "text": "I must see the dentist now, nurse.",
      "translation": "",
      "startTime": 57.5,
      "endTime": 62.81
    },
    {
      "id": "s13",
      "role": "Man",
      "text": "The dentist is very busy at the moment.",
      "translation": "",
      "startTime": 62.82,
      "endTime": 67.33
    },
    {
      "id": "s14",
      "role": "Woman",
      "text": "Can you come at 2 p.m. ?",
      "translation": "",
      "startTime": 67.34,
      "endTime": 71.23
    },
    {
      "id": "s15",
      "role": "Man",
      "text": "That's very late.",
      "translation": "",
      "startTime": 71.24,
      "endTime": 74.32
    },
    {
      "id": "s16",
      "role": "Woman",
      "text": "Can the dentist see me now?",
      "translation": "",
      "startTime": 74.33,
      "endTime": 77.65
    },
    {
      "id": "s17",
      "role": "Man",
      "text": "I'm afraid that he can't, Mr. Croft.",
      "translation": "",
      "startTime": 77.66,
      "endTime": 82.13
    },
    {
      "id": "s18",
      "role": "Woman",
      "text": "Can't you wait till this afternoon?",
      "translation": "",
      "startTime": 82.14,
      "endTime": 86.23
    },
    {
      "id": "s19",
      "role": "Man",
      "text": "I can wait, but my toothache can't!",
      "translation": "",
      "startTime": 86.24,
      "endTime": 91.24
    }
  ]
}