      "id": "intro_2",
      "role": "Narrator",
      "text": "Where's Sally?",
      "translation": "",
      "startTime": 3.04,
      "endTime": 5.64
    },
    {
      "id": "intro_3",
      "role": "Narrator",
      "text": "Listen to the tape then answer this question.",
      "translation": "",
      "startTime": 5.65,
      "endTime": 11.44
    },
    {
      "id": "intro_4",
      "role": "Narrator",
      "text": "Is the cat climbing the tree?",
      "translation": "",
      "startTime": 11.45,
      "endTime": 15.83
    },
    {
      "id": "s1",
      "role": "Man",
      "text": "Where's Sally, Jack?",
      "translation": "",
      "startTime": 15.84,
      "endTime": 18.66
    },
    {
      "id": "s2",
      "role": "Woman",
      "text": "She's in the garden, Jean.",
      "translation": "",
      "startTime": 18.67,
      "endTime": 21.44
    },
    {
      "id": "s3",
      "role": "Narrator",
      "text": "What's she doing?",
      "translation": "",
      "startTime": 21.45,
      "endTime": 23.46
    },
    {
      "id": "s4",
      "role": "Man",
      "text": "She's sitting under the tree.",
      "translation": "",
      "startTime": 23.47,
      "endTime": 26.55
    },
    {
      "id": "s5",
      "role": "Woman",
      "text": "Is Tim in the garden, too?",
      "translation": "",
      "startTime": 26.56,
      "endTime": 29.87
    },
    {
      "id": "s6",
      "role": "Man",
      "text": "Yes, he is.",
      "translation": "",
      "startTime": 29.88,
      "endTime": 31.81
    },
    {
      "id": "s7",
      "role": "Woman",
      "text": "He's climbing the tree.",
      "translation": "",
      "startTime": 31.82,
      "endTime": 34.67
    },
    {
      "id": "s8",
      "role": "Man",
      "text": "I beg your pardon?",
      "translation": "",
      "startTime": 34.68,
      "endTime": 36.9
    },
    {
      "id": "s9",
      "role": "Narrator",
      "text": "Who's climbing the tree?",
      "translation": "",
      "startTime": 36.91,
      "endTime": 39.46
    },
    {
      "id": "s10",
      "role": "Man",
      "text": "Tim is.",
      "translation": "",
      "startTime": 39.47,
      "endTime": 41.4
    },
    {
      "id": "s11",
      "role": "Narrator",
      "text": "What about the dog?",
      "translation": "",
      "startTime": 41.41,
      "endTime": 44.24
    },
    {
      "id": "s12",
      "role": "Man",
      "text": "The dog's in the garden, too.",
      "translation": "",
      "startTime": 44.25,
      "endTime": 46.88
    },
    {
      "id": "s13",
      "role": "Woman",
      "text": "It's running across the grass.",
      "translation": "",
      "startTime": 46.89,
      "endTime": 50.01
    },
    {
      "id": "s14",
      "role": "Man",
      "text": "It's running after a cat.",
      "translation": "",
      "startTime": 50.02,
      "endTime": 55.02
    }
  ]
