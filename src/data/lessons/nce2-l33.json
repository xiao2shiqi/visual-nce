    {
      "id": "intro_1",
      "role": "Narrator",
      "text": "Why was the girl in hospital?",
      "translation": "",
      "startTime": 10.68,
      "endTime": 14.61
    },
    {
      "id": "intro_2",
      "role": "Narrator",
      "text": "Nearly a week passed before",
      "translation": "",
      "startTime": 14.62,
      "endTime": 16.95
    },
    {
      "id": "intro_3",
      "role": "Narrator",
      "text": "the girl was able to explain",
      "translation": "",
      "startTime": 16.96,
      "endTime": 19.53
    },
    {
      "id": "intro_4",
      "role": "Narrator",
      "text": "what had happened to her.",
      "translation": "",
      "startTime": 19.54,
      "endTime": 23.14,
      "analysis": {
        "words": [
          {
            "word": "What",
            "pos": "pron.",
            "meaning": "什么"
          }
        ]
      }
    },
    {
      "id": "s1",
      "role": "Man",
      "text": "One afternoon she set out",
      "translation": "",
      "startTime": 23.15,
      "endTime": 25.69
    },
    {
      "id": "s2",
      "role": "Woman",
      "text": "from the coast in a small boat",
      "translation": "",
      "startTime": 25.7,
      "endTime": 28.21
    },
    {
      "id": "s3",
      "role": "Man",
      "text": "and was caught in a storm.",
      "translation": "",
      "startTime": 28.22,
      "endTime": 31.48
    },
    {
      "id": "s4",
      "role": "Woman",
      "text": "Towards evening,",
      "translation": "",
      "startTime": 31.49,
      "endTime": 33.08
    },
    {
      "id": "s5",
      "role": "Man",
      "text": "the boat struck a rock and",
      "translation": "",
      "startTime": 33.09,
      "endTime": 35.12
    },
    {
      "id": "s6",
      "role": "Woman",
      "text": "the girl jumped into the sea.",
      "translation": "",
      "startTime": 35.13,
      "endTime": 38.29
    },
    {
      "id": "s7",
      "role": "Man",
      "text": "Then she swam to the shore",
      "translation": "",
      "startTime": 38.3,
      "endTime": 40.62
    },
    {
      "id": "s8",
      "role": "Woman",
      "text": "after spending the whole night in the water.",
      "translation": "",
      "startTime": 40.63,
      "endTime": 44.7
    },
    {
      "id": "s9",
      "role": "Man",
      "text": "During that time she covered",
      "translation": "",
      "startTime": 44.71,
      "endTime": 47.49,
      "analysis": {
        "words": [
          {
            "word": "That",
            "pos": "pron.",
            "meaning": "那个"
          }
        ]
      }
    },
    {
      "id": "s10",
      "role": "Woman",
      "text": "a distance of eight miles.",
      "translation": "",
      "startTime": 47.5,
      "endTime": 50.8
    },
    {
      "id": "s11",
      "role": "Man",
      "text": "Early next morning,",
      "translation": "",
      "startTime": 50.81,
      "endTime": 52.89
    },
    {
      "id": "s12",
      "role": "Woman",
      "text": "she saw a light ahead.",
      "translation": "",
      "startTime": 52.9,
      "endTime": 55.79
    },
    {
      "id": "s13",
      "role": "Man",
      "text": "She knew she was near the shore",
      "translation": "",
      "startTime": 55.8,
      "endTime": 58.89
    },
    {
      "id": "s14",
      "role": "Woman",
      "text": "because the light was high up on the cliffs.",
      "translation": "",
      "startTime": 58.9,
      "endTime": 63.5
    },
    {
      "id": "s15",
      "role": "Man",
      "text": "On arriving at the shore,",
      "translation": "",
      "startTime": 63.51,
      "endTime": 66.15,
      "analysis": {
        "words": [
          {
            "word": "At",
            "pos": "prep.",
            "meaning": "在...（小地点/时刻）"
          }
        ]
      }
    },
    {
      "id": "s16",
      "role": "Woman",
      "text": "the girl struggled up the cliff towards",
      "translation": "",
      "startTime": 66.16,
      "endTime": 69.55
    },
    {
      "id": "s17",
      "role": "Man",
      "text": "the light she had seen.",
      "translation": "",
      "startTime": 69.56,
      "endTime": 72.08
    },
    {
      "id": "s18",
      "role": "Woman",
      "text": "That was all she remembered.",
      "translation": "",
      "startTime": 72.09,
      "endTime": 75.16,
      "analysis": {
        "words": [
          {
            "word": "That",
            "pos": "pron.",
            "meaning": "那个"
          }
        ]
      }
    },
    {
      "id": "s19",
      "role": "Man",
      "text": "When she woke up a day later,",
      "translation": "",
      "startTime": 75.17,
      "endTime": 77.96
    },
    {
      "id": "s20",
      "role": "Woman",
      "text": "she found herself in hospital.",
      "translation": "",
      "startTime": 77.97,
      "endTime": 82.97
    }
  ]
}
