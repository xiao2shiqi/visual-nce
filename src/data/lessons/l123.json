    {
      "id": "intro_2",
      "role": "Narrator",
      "text": "A trip to Australia",
      "translation": "",
      "startTime": 4,
      "endTime": 7.34
    },
    {
      "id": "intro_3",
      "role": "Narrator",
      "text": "Listen to the tape then answer this question.",
      "translation": "",
      "startTime": 7.35,
      "endTime": 12.93
    },
    {
      "id": "intro_4",
      "role": "Narrator",
      "text": "Who is the man with the beard?",
      "translation": "",
      "startTime": 12.94,
      "endTime": 17.82
    },
    {
      "id": "s1",
      "role": "Man",
      "text": "Look, Scott.",
      "translation": "",
      "startTime": 17.83,
      "endTime": 19.98
    },
    {
      "id": "s2",
      "role": "Woman",
      "text": "This is a photograph I took during my trip to Australia.",
      "translation": "",
      "startTime": 19.99,
      "endTime": 26.63
    },
    {
      "id": "s3",
      "role": "Man",
      "text": "Let me see it, Mike.",
      "translation": "",
      "startTime": 26.64,
      "endTime": 29.23
    },
    {
      "id": "s4",
      "role": "Woman",
      "text": "This is a good photograph.",
      "translation": "",
      "startTime": 29.24,
      "endTime": 31.94
    },
    {
      "id": "s5",
      "role": "Narrator",
      "text": "Who are these people?",
      "translation": "",
      "startTime": 31.95,
      "endTime": 35.03
    },
    {
      "id": "s6",
      "role": "Man",
      "text": "They're people I met during the trip.",
      "translation": "",
      "startTime": 35.04,
      "endTime": 39.48
    },
    {
      "id": "s7",
      "role": "Woman",
      "text": "That's the ship we travelled on.",
      "translation": "",
      "startTime": 39.49,
      "endTime": 42.38
    },
    {
      "id": "s8",
      "role": "Narrator",
      "text": "What a beautiful ship!",
      "translation": "",
      "startTime": 42.39,
      "endTime": 45
    },
    {
      "id": "s9",
      "role": "Narrator",
      "text": "Who's this?",
      "translation": "",
      "startTime": 45.01,
      "endTime": 47.11
    },
    {
      "id": "s10",
      "role": "Man",
      "text": "That's the man I told you about.",
      "translation": "",
      "startTime": 47.12,
      "endTime": 50.18
    },
    {
      "id": "s11",
      "role": "Woman",
      "text": "Remember?",
      "translation": "",
      "startTime": 50.19,
      "endTime": 51.57
    },
    {
      "id": "s12",
      "role": "Man",
      "text": "Ah yes.",
      "translation": "",
      "startTime": 51.58,
      "endTime": 53.09
    },
    {
      "id": "s13",
      "role": "Woman",
      "text": "The one who offered you a job in Australia.",
      "translation": "",
      "startTime": 53.1,
      "endTime": 57.43
    },
    {
      "id": "s14",
      "role": "Man",
      "text": "That's right.",
      "translation": "",
      "startTime": 57.44,
      "endTime": 58.89
    },
    {
      "id": "s15",
      "role": "Narrator",
      "text": "Who's this?",
      "translation": "",
      "startTime": 58.9,
      "endTime": 60.63
    },
    {
      "id": "s16",
      "role": "Man",
      "text": "Guess!",
      "translation": "",
      "startTime": 60.64,
      "endTime": 61.73
    },
    {
      "id": "s17",
      "role": "Woman",
      "text": "It's not you, is it?",
      "translation": "",
      "startTime": 61.74,
      "endTime": 64.16
    },
    {
      "id": "s18",
      "role": "Man",
      "text": "That's right.",
      "translation": "",
      "startTime": 64.17,
      "endTime": 66.72
    },
    {
      "id": "s19",
      "role": "Woman",
      "text": "I grew a beard during the trip, but I shaved it off when I came home.",
      "translation": "",
      "startTime": 66.73,
      "endTime": 73.49
    },
    {
      "id": "s20",
      "role": "Narrator",
      "text": "Why did you shave it off?",
      "translation": "",
      "startTime": 73.5,
      "endTime": 76.15
    },
    {
      "id": "s21",
      "role": "Man",
      "text": "My wife didn't like it!",
      "translation": "",
      "startTime": 76.16,
      "endTime": 81.16
    }
  ]
}