    {
      "id": "intro_2",
      "role": "Narrator",
      "text": "A bad cold",
      "translation": "",
      "startTime": 3.06,
      "endTime": 5.91
    },
    {
      "id": "intro_3",
      "role": "Narrator",
      "text": "Listen to the tape then answer this question.",
      "translation": "",
      "startTime": 5.92,
      "endTime": 11.14
    },
    {
      "id": "intro_4",
      "role": "Narrator",
      "text": "What is good news for Jimmy?",
      "translation": "",
      "startTime": 11.15,
      "endTime": 16.54
    },
    {
      "id": "s1",
      "role": "Man",
      "text": "Where's Jimmy?",
      "translation": "",
      "startTime": 16.55,
      "endTime": 18.35
    },
    {
      "id": "s2",
      "role": "Woman",
      "text": "He's in bed.",
      "translation": "",
      "startTime": 18.36,
      "endTime": 20.68
    },
    {
      "id": "s3",
      "role": "Narrator",
      "text": "What's the matter with him?",
      "translation": "",
      "startTime": 20.69,
      "endTime": 23.13
    },
    {
      "id": "s4",
      "role": "Man",
      "text": "He feels ill.",
      "translation": "",
      "startTime": 23.14,
      "endTime": 25.83
    },
    {
      "id": "s5",
      "role": "Woman",
      "text": "He looks ill.",
      "translation": "",
      "startTime": 25.84,
      "endTime": 27.85
    },
    {
      "id": "s6",
      "role": "Man",
      "text": "We must call the doctor.",
      "translation": "",
      "startTime": 27.86,
      "endTime": 31.04
    },
    {
      "id": "s7",
      "role": "Woman",
      "text": "Yes, we must.",
      "translation": "",
      "startTime": 31.05,
      "endTime": 34.11
    },
    {
      "id": "s8",
      "role": "Man",
      "text": "Can you remember the doctor's telephone number?",
      "translation": "",
      "startTime": 34.12,
      "endTime": 38.74
    },
    {
      "id": "s9",
      "role": "Woman",
      "text": "Yes. It's 09754.",
      "translation": "",
      "startTime": 38.75,
      "endTime": 45.77
    },
    {
      "id": "s10",
      "role": "Man",
      "text": "Open your mouth, Jimmy.",
      "translation": "",
      "startTime": 45.78,
      "endTime": 48.36
    },
    {
      "id": "s11",
      "role": "Woman",
      "text": "Show me your tongue. Say, 'Ah'.",
      "translation": "",
      "startTime": 48.37,
      "endTime": 53.62
    },
    {
      "id": "s12",
      "role": "Narrator",
      "text": "What's the matter with him, doctor?",
      "translation": "",
      "startTime": 53.63,
      "endTime": 57.21
    },
    {
      "id": "s13",
      "role": "Man",
      "text": "He has a bad cold, Mr. Williams, so he must stay in bed for a week.",
      "translation": "",
      "startTime": 57.22,
      "endTime": 65.8
    },
    {
      "id": "s14",
      "role": "Woman",
      "text": "That's good news for Jimmy.",
      "translation": "",
      "startTime": 65.81,
      "endTime": 69.34
    },
    {
      "id": "s15",
      "role": "Man",
      "text": "Good news? Why?",
      "translation": "",
      "startTime": 69.35,
      "endTime": 72.35
    },
    {
      "id": "s16",
      "role": "Woman",
      "text": "Because he doesn't like school!",
      "translation": "",
      "startTime": 72.36,
      "endTime": 77.36
    }
  ]
}