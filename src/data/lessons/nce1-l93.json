  "segments": [
    {
      "id": "intro_1",
      "role": "Narrator",
      "text": "Lesson 93",
      "translation": "",
      "startTime": 0.51,
      "endTime": 3
    },
    {
      "id": "intro_2",
      "role": "Narrator",
      "text": "Our new neighbour",
      "translation": "",
      "startTime": 3.01,
      "endTime": 6.02
    },
    {
      "id": "intro_3",
      "role": "Narrator",
      "text": "Listen to the tape then answer this question.",
      "translation": "",
      "startTime": 6.03,
      "endTime": 11.97
    },
    {
      "id": "intro_4",
      "role": "Narrator",
      "text": "Why is Nigel a lucky man?",
      "translation": "",
      "startTime": 11.98,
      "endTime": 17.18
    },
    {
      "id": "s1",
      "role": "Man",
      "text": "Nigel is our new next-door neighbour.",
      "translation": "",
      "startTime": 17.19,
      "endTime": 21.63
    },
    {
      "id": "s2",
      "role": "Woman",
      "text": "He's a pilot.",
      "translation": "",
      "startTime": 21.64,
      "endTime": 24.15
    },
    {
      "id": "s3",
      "role": "Man",
      "text": "He was in the R.A.F.",
      "translation": "",
      "startTime": 24.16,
      "endTime": 28.28
    },
    {
      "id": "s4",
      "role": "Woman",
      "text": "He will fly to New York next month.",
      "translation": "",
      "startTime": 28.29,
      "endTime": 33.18
    },
    {
      "id": "s5",
      "role": "Man",
      "text": "The month after next he'll fly to Tokyo.",
      "translation": "",
      "startTime": 33.19,
      "endTime": 38.61
    },
    {
      "id": "s6",
      "role": "Woman",
      "text": "At the moment, he's in Madrid.",
      "translation": "",
      "startTime": 38.62,
      "endTime": 42.3
    },
    {
      "id": "s7",
      "role": "Man",
      "text": "He flew to Spain a week ago.",
      "translation": "",
      "startTime": 42.31,
      "endTime": 46.57
    },
    {
      "id": "s8",
      "role": "Woman",
      "text": "He'll return to London the week after next.",
      "translation": "",
      "startTime": 46.58,
      "endTime": 52.14
    },
    {
      "id": "s9",
      "role": "Man",
      "text": "He's only forty-one years old,",
      "translation": "",
      "startTime": 52.15,
      "endTime": 55.88
    },
    {
      "id": "s10",
      "role": "Woman",
      "text": "and he has already been to nearly every country in the world.",
      "translation": "",
      "startTime": 55.89,
      "endTime": 63.41
    },
    {
      "id": "s11",
      "role": "Man",
      "text": "Nigel is a very lucky man.",
      "translation": "",
      "startTime": 63.42,
      "endTime": 67.19
    },
    {
      "id": "s12",
      "role": "Woman",
      "text": "But his wife isn't very lucky.",
      "translation": "",
      "startTime": 67.2,
      "endTime": 70.51
    },
    {
      "id": "s13",
      "role": "Man",
      "text": "She usually stays at home!",
      "translation": "",
      "startTime": 70.52,
      "endTime": 75.52
    }
  ]
}