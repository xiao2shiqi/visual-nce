    {
      "id": "intro_2",
      "role": "Narrator",
      "text": "Going on holiday",
      "translation": "",
      "startTime": 3.18,
      "endTime": 6.51
    },
    {
      "id": "intro_3",
      "role": "Narrator",
      "text": "Listen to the tape then answer this question.",
      "translation": "",
      "startTime": 6.52,
      "endTime": 12.14
    },
    {
      "id": "intro_4",
      "role": "Narrator",
      "text": "Where did Sam go for his holiday this year?",
      "translation": "",
      "startTime": 12.15,
      "endTime": 18.99
    },
    {
      "id": "s1",
      "role": "Man",
      "text": "Hello, Sam.",
      "translation": "",
      "startTime": 19,
      "endTime": 20.58
    },
    {
      "id": "s2",
      "role": "Woman",
      "text": "Come in.",
      "translation": "",
      "startTime": 20.59,
      "endTime": 23.05
    },
    {
      "id": "s3",
      "role": "Man",
      "text": "Hi, Sam.",
      "translation": "",
      "startTime": 23.06,
      "endTime": 24.7
    },
    {
      "id": "s4",
      "role": "Woman",
      "text": "We're having lunch.",
      "translation": "",
      "startTime": 24.71,
      "endTime": 26.87
    },
    {
      "id": "s5",
      "role": "Man",
      "text": "Do you want to have lunch with us?",
      "translation": "",
      "startTime": 26.88,
      "endTime": 30.97
    },
    {
      "id": "s6",
      "role": "Woman",
      "text": "No, thank you, Tom.",
      "translation": "",
      "startTime": 30.98,
      "endTime": 33.17
    },
    {
      "id": "s7",
      "role": "Man",
      "text": "I've already had lunch.",
      "translation": "",
      "startTime": 33.18,
      "endTime": 35.96
    },
    {
      "id": "s8",
      "role": "Woman",
      "text": "I had lunch at half past twelve.",
      "translation": "",
      "startTime": 35.97,
      "endTime": 39.97
    },
    {
      "id": "s9",
      "role": "Man",
      "text": "Have a cup of coffee then.",
      "translation": "",
      "startTime": 39.98,
      "endTime": 42.92
    },
    {
      "id": "s10",
      "role": "Woman",
      "text": "I've just had a cup, thank you.",
      "translation": "",
      "startTime": 42.93,
      "endTime": 46.24
    },
    {
      "id": "s11",
      "role": "Man",
      "text": "I had one after my lunch.",
      "translation": "",
      "startTime": 46.25,
      "endTime": 49.95
    },
    {
      "id": "s12",
      "role": "Woman",
      "text": "Let's go into the living room, Carol.",
      "translation": "",
      "startTime": 49.96,
      "endTime": 53.74
    },
    {
      "id": "s13",
      "role": "Man",
      "text": "We can have our coffee there.",
      "translation": "",
      "startTime": 53.75,
      "endTime": 57.11
    },
    {
      "id": "s14",
      "role": "Woman",
      "text": "Excuse the mess, Sam.",
      "translation": "",
      "startTime": 57.12,
      "endTime": 60.32
    },
    {
      "id": "s15",
      "role": "Man",
      "text": "This room's very untidy.",
      "translation": "",
      "startTime": 60.33,
      "endTime": 63.5
    },
    {
      "id": "s16",
      "role": "Woman",
      "text": "We're packing our suitcases.",
      "translation": "",
      "startTime": 63.51,
      "endTime": 66.41
    },
    {
      "id": "s17",
      "role": "Man",
      "text": "We're going to leave tomorrow.",
      "translation": "",
      "startTime": 66.42,
      "endTime": 69.84
    },
    {
      "id": "s18",
      "role": "Woman",
      "text": "Tom and I are going to have a holiday.",
      "translation": "",
      "startTime": 69.85,
      "endTime": 74.32
    },
    {
      "id": "s19",
      "role": "Man",
      "text": "Aren't you lucky!",
      "translation": "",
      "startTime": 74.33,
      "endTime": 77.48
    },
    {
      "id": "s20",
      "role": "Woman",
      "text": "When are you going to have a holiday, Sam?",
      "translation": "",
      "startTime": 77.49,
      "endTime": 81.57
    },
    {
      "id": "s21",
      "role": "Man",
      "text": "I don't know.",
      "translation": "",
      "startTime": 81.58,
      "endTime": 83.45
    },
    {
      "id": "s22",
      "role": "Woman",
      "text": "I've already had my holiday this year.",
      "translation": "",
      "startTime": 83.46,
      "endTime": 88.09
    },
    {
      "id": "s23",
      "role": "Man",
      "text": "Where did you go?",
      "translation": "",
      "startTime": 88.1,
      "endTime": 90.21
    },
    {
      "id": "s24",
      "role": "Woman",
      "text": "I stayed at home!",
      "translation": "",
      "startTime": 90.22,
      "endTime": 95.22
    }
  ]
}