      "translation": "",
      "startTime": 2.83,
      "endTime": 10.4
    },
    {
      "id": "intro_3",
      "role": "Narrator",
      "text": "What is one of the features of modern camping where nationality is concerned?",
      "translation": "",
      "startTime": 10.41,
      "endTime": 18.09,
      "analysis": {
        "grammar": [
          {
            "text": "What",
            "label": "疑问词"
          },
          {
