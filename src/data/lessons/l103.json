    {
      "id": "intro_2",
      "role": "Narrator",
      "text": "The French test",
      "translation": "",
      "startTime": 3.54,
      "endTime": 6.76
    },
    {
      "id": "intro_3",
      "role": "Narrator",
      "text": "Listen to the tape then answer this question.",
      "translation": "",
      "startTime": 6.77,
      "endTime": 12.81
    },
    {
      "id": "intro_4",
      "role": "Narrator",
      "text": "How long did the exam last?",
      "translation": "",
      "startTime": 12.82,
      "endTime": 17.9
    },
    {
      "id": "s1",
      "role": "Narrator",
      "text": "How was the exam, Richard?",
      "translation": "",
      "startTime": 17.91,
      "endTime": 20.79
    },
    {
      "id": "s2",
      "role": "Man",
      "text": "Not too bad.",
      "translation": "",
      "startTime": 20.8,
      "endTime": 22.55
    },
    {
      "id": "s3",
      "role": "Woman",
      "text": "I think I passed in English and Mathematics.",
      "translation": "",
      "startTime": 22.56,
      "endTime": 27.27
    },
    {
      "id": "s4",
      "role": "Man",
      "text": "The questions were very easy.",
      "translation": "",
      "startTime": 27.28,
      "endTime": 30.33
    },
    {
      "id": "s5",
      "role": "Narrator",
      "text": "How about you, Gary?",
      "translation": "",
      "startTime": 30.34,
      "endTime": 33.98
    },
    {
      "id": "s6",
      "role": "Man",
      "text": "The English and Maths papers weren't easy enough for me.",
      "translation": "",
      "startTime": 33.99,
      "endTime": 39.97
    },
    {
      "id": "s7",
      "role": "Woman",
      "text": "I hope I haven't failed.",
      "translation": "",
      "startTime": 39.98,
      "endTime": 42.78
    },
    {
      "id": "s8",
      "role": "Man",
      "text": "I think I failed the French paper.",
      "translation": "",
      "startTime": 42.79,
      "endTime": 46.59
    },
    {
      "id": "s9",
      "role": "Woman",
      "text": "I could answer sixteen of the questions.",
      "translation": "",
      "startTime": 46.6,
      "endTime": 50.55
    },
    {
      "id": "s10",
      "role": "Man",
      "text": "They were very easy.",
      "translation": "",
      "startTime": 50.56,
      "endTime": 53.02
    },
    {
      "id": "s11",
      "role": "Woman",
      "text": "But I couldn't answer the rest.",
      "translation": "",
      "startTime": 53.03,
      "endTime": 56.2
    },
    {
      "id": "s12",
      "role": "Man",
      "text": "They were too difficult for me.",
      "translation": "",
      "startTime": 56.21,
      "endTime": 60.28
    },
    {
      "id": "s13",
      "role": "Woman",
      "text": "French tests are awful, aren't they?",
      "translation": "",
      "startTime": 60.29,
      "endTime": 63.84
    },
    {
      "id": "s14",
      "role": "Man",
      "text": "I hate them.",
      "translation": "",
      "startTime": 63.85,
      "endTime": 65.71
    },
    {
      "id": "s15",
      "role": "Woman",
      "text": "I'm sure I've got a low mark.",
      "translation": "",
      "startTime": 65.72,
      "endTime": 70.12
    },
    {
      "id": "s16",
      "role": "Man",
      "text": "Oh, cheer up!",
      "translation": "",
      "startTime": 70.13,
      "endTime": 72.72
    },
    {
      "id": "s17",
      "role": "Woman",
      "text": "Perhaps we didn't do too badly.",
      "translation": "",
      "startTime": 72.73,
      "endTime": 77
    },
    {
      "id": "s18",
      "role": "Man",
      "text": "The guy next to me wrote his name at the top of the paper.",
      "translation": "",
      "startTime": 77.01,
      "endTime": 82.52
    },
    {
      "id": "s19",
      "role": "Woman",
      "text": "Yes?",
      "translation": "",
      "startTime": 82.53,
      "endTime": 84.1
    },
    {
      "id": "s20",
      "role": "Man",
      "text": "Then he sat there and looked at it for three hours!",
      "translation": "",
      "startTime": 84.11,
      "endTime": 89.35
    },
    {
      "id": "s21",
      "role": "Woman",
      "text": "He didn't write a word!",
      "translation": "",
      "startTime": 89.36,
      "endTime": 94.36
    }
  ]
}