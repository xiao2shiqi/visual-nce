    {
      "id": "intro_2",
      "role": "Narrator",
      "text": "Carol's shopping list",
      "translation": "",
      "startTime": 3.52,
      "endTime": 6.71
    },
    {
      "id": "intro_3",
      "role": "Narrator",
      "text": "Listen to the tape then answer this question.",
      "translation": "",
      "startTime": 6.72,
      "endTime": 12.23
    },
    {
      "id": "intro_4",
      "role": "Narrator",
      "text": "What is Carol not going to buy?",
      "translation": "",
      "startTime": 12.24,
      "endTime": 17.9
    },
    {
      "id": "s1",
      "role": "Narrator",
      "text": "What are you doing, Carol?",
      "translation": "",
      "startTime": 17.91,
      "endTime": 21.72
    },
    {
      "id": "s2",
      "role": "Man",
      "text": "I'm making a shopping list, Tom.",
      "translation": "",
      "startTime": 21.73,
      "endTime": 26.47
    },
    {
      "id": "s3",
      "role": "Narrator",
      "text": "What do we need?",
      "translation": "",
      "startTime": 26.48,
      "endTime": 29.56
    },
    {
      "id": "s4",
      "role": "Man",
      "text": "We need a lot of things this week.",
      "translation": "",
      "startTime": 29.57,
      "endTime": 33.61
    },
    {
      "id": "s5",
      "role": "Woman",
      "text": "I must go to the grocer's.",
      "translation": "",
      "startTime": 33.62,
      "endTime": 37.24
    },
    {
      "id": "s6",
      "role": "Man",
      "text": "We haven't got much tea or coffee, and we haven't got any sugar or jam.",
      "translation": "",
      "startTime": 37.25,
      "endTime": 46.58
    },
    {
      "id": "s7",
      "role": "Narrator",
      "text": "What about vegetables?",
      "translation": "",
      "startTime": 46.59,
      "endTime": 49.37
    },
    {
      "id": "s8",
      "role": "Man",
      "text": "I must go to the greengroce's.",
      "translation": "",
      "startTime": 49.38,
      "endTime": 53.54
    },
    {
      "id": "s9",
      "role": "Woman",
      "text": "We haven't got many tomatoes, but we've got a lot of potatoes.",
      "translation": "",
      "startTime": 53.55,
      "endTime": 61.49
    },
    {
      "id": "s10",
      "role": "Man",
      "text": "I must go to the butcher's, too.",
      "translation": "",
      "startTime": 61.5,
      "endTime": 65.44
    },
    {
      "id": "s11",
      "role": "Woman",
      "text": "We need some meat.",
      "translation": "",
      "startTime": 65.45,
      "endTime": 68.22
    },
    {
      "id": "s12",
      "role": "Man",
      "text": "We haven't got any meat at all.",
      "translation": "",
      "startTime": 68.23,
      "endTime": 73.33
    },
    {
      "id": "s13",
      "role": "Woman",
      "text": "Have we got any beer and wine?",
      "translation": "",
      "startTime": 73.34,
      "endTime": 77.29
    },
    {
      "id": "s14",
      "role": "Man",
      "text": "No, we haven't.",
      "translation": "",
      "startTime": 77.3,
      "endTime": 79.76
    },
    {
      "id": "s15",
      "role": "Woman",
      "text": "And I'm not going to get any!",
      "translation": "",
      "startTime": 79.77,
      "endTime": 83.53
    },
    {
      "id": "s16",
      "role": "Man",
      "text": "I hope that you've got some money.",
      "translation": "",
      "startTime": 83.54,
      "endTime": 87.02
    },
    {
      "id": "s17",
      "role": "Woman",
      "text": "I haven't got much.",
      "translation": "",
      "startTime": 87.03,
      "endTime": 89.54
    },
    {
      "id": "s18",
      "role": "Man",
      "text": "Well, I haven't got much either!",
      "translation": "",
      "startTime": 89.55,
      "endTime": 94.55
    }
  ]
}