    {
      "id": "intro_2",
      "role": "Narrator",
      "text": "A puma at large",
      "translation": "",
      "startTime": 2.36,
      "endTime": 9.8,
      "analysis": {
        "words": [
          {
            "word": "At",
            "pos": "prep.",
