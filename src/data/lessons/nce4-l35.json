    },
    {
      "id": "s3",
      "role": "Man",
      "text": "Because the Moon has only an eightieth of the Earth's mass,",
      "translation": "",
      "startTime": 36.79,
      "endTime": 41.42
    },
    {
      "id": "s4",
      "role": "Woman",
