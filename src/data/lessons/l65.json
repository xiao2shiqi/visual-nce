  "segments": [
    {
      "id": "intro_1",
      "role": "Narrator",
      "text": "Lesson 65",
      "translation": "",
      "startTime": 0.73,
      "endTime": 3.34
    },
    {
      "id": "intro_2",
      "role": "Narrator",
      "text": "Not a baby",
      "translation": "",
      "startTime": 3.35,
      "endTime": 6.24
    },
    {
      "id": "intro_3",
      "role": "Narrator",
      "text": "Listen to the tape then answer this question.",
      "translation": "",
      "startTime": 6.25,
      "endTime": 11.57
    },
    {
      "id": "intro_4",
      "role": "Narrator",
      "text": "Does Jill take the key to the front door?",
      "translation": "",
      "startTime": 11.58,
      "endTime": 17.66
    },
    {
      "id": "s1",
      "role": "Narrator",
      "text": "What are you going to do this evening, Jill?",
      "translation": "",
      "startTime": 17.67,
      "endTime": 22.62
    },
    {
      "id": "s2",
      "role": "Man",
      "text": "I'm going to meet some friends, Dad.",
      "translation": "",
      "startTime": 22.63,
      "endTime": 27.14
    },
    {
      "id": "s3",
      "role": "Woman",
      "text": "You mustn't come home late.",
      "translation": "",
      "startTime": 27.15,
      "endTime": 30.18
    },
    {
      "id": "s4",
      "role": "Man",
      "text": "You must be home at half past ten.",
      "translation": "",
      "startTime": 30.19,
      "endTime": 34.86
    },
    {
      "id": "s5",
      "role": "Woman",
      "text": "I can't get home so early, Dad!",
      "translation": "",
      "startTime": 34.87,
      "endTime": 39.4
    },
    {
      "id": "s6",
      "role": "Man",
      "text": "Can I have the key to the front door, please?",
      "translation": "",
      "startTime": 39.41,
      "endTime": 44.47
    },
    {
      "id": "s7",
      "role": "Woman",
      "text": "No, you can't.",
      "translation": "",
      "startTime": 44.48,
      "endTime": 47.6
    },
    {
      "id": "s8",
      "role": "Man",
      "text": "Jill's eighteen years old, Tom.",
      "translation": "",
      "startTime": 47.61,
      "endTime": 51.88
    },
    {
      "id": "s9",
      "role": "Woman",
      "text": "She's not a baby.",
      "translation": "",
      "startTime": 51.89,
      "endTime": 54.73
    },
    {
      "id": "s10",
      "role": "Man",
      "text": "Give her the key.",
      "translation": "",
      "startTime": 54.74,
      "endTime": 56.97
    },
    {
      "id": "s11",
      "role": "Woman",
      "text": "She always comes home early.",
      "translation": "",
      "startTime": 56.98,
      "endTime": 61.07
    },
    {
      "id": "s12",
      "role": "Man",
      "text": "Oh, all right!",
      "translation": "",
      "startTime": 61.08,
      "endTime": 64.44
    },
    {
      "id": "s13",
      "role": "Woman",
      "text": "Here you are.",
      "translation": "",
      "startTime": 64.45,
      "endTime": 66.26
    },
    {
      "id": "s14",
      "role": "Man",
      "text": "But you mustn't come home after a quarter past eleven.",
      "translation": "",
      "startTime": 66.27,
      "endTime": 71.51
    },
    {
      "id": "s15",
      "role": "Woman",
      "text": "Do you hear?",
      "translation": "",
      "startTime": 71.52,
      "endTime": 73.56
    },
    {
      "id": "s16",
      "role": "Man",
      "text": "Yes, Dad.",
      "translation": "",
      "startTime": 73.57,
      "endTime": 75.81
    },
    {
      "id": "s17",
      "role": "Woman",
      "text": "Thanks, Mum.",
      "translation": "",
      "startTime": 75.82,
      "endTime": 77.8
    },
    {
      "id": "s18",
      "role": "Man",
      "text": "That's all right.",
      "translation": "",
      "startTime": 77.81,
      "endTime": 79.91
    },
    {
      "id": "s19",
      "role": "Woman",
      "text": "Goodbye.",
      "translation": "",
      "startTime": 79.92,
      "endTime": 81.51
    },
    {
      "id": "s20",
      "role": "Man",
      "text": "Enjoy yourself!",
      "translation": "",
      "startTime": 81.52,
      "endTime": 84.38
    },
    {
      "id": "s21",
      "role": "Woman",
      "text": "We always enjoy ourselves, Mum.",
      "translation": "",
      "startTime": 84.39,
      "endTime": 88.29
    },
    {
      "id": "s22",
      "role": "Man",
      "text": "Bye-bye.",
      "translation": "",
      "startTime": 88.3,
      "endTime": 93.3
    }
  ]
}