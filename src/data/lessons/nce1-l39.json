    {
      "id": "intro_2",
      "role": "Narrator",
      "text": "Don't drop it!",
      "translation": "",
      "startTime": 3.11,
      "endTime": 6.27
    },
    {
      "id": "intro_3",
      "role": "Narrator",
      "text": "Listen to the tape then answer this question.",
      "translation": "",
      "startTime": 6.28,
      "endTime": 12.43
    },
    {
      "id": "intro_4",
      "role": "Narrator",
      "text": "Where does Sam put the vase in the end?",
      "translation": "",
      "startTime": 12.44,
      "endTime": 18.54
    },
    {
      "id": "s1",
      "role": "Narrator",
      "text": "What are you going to do with that vase, Penny?",
      "translation": "",
      "startTime": 18.55,
      "endTime": 24.01
    },
    {
      "id": "s2",
      "role": "Man",
      "text": "I'm going to put it on this table, Sam.",
      "translation": "",
      "startTime": 24.02,
      "endTime": 29.14
    },
    {
      "id": "s3",
      "role": "Woman",
      "text": "Don't do that.",
      "translation": "",
      "startTime": 29.15,
      "endTime": 31.3
    },
    {
      "id": "s4",
      "role": "Man",
      "text": "Give it to me.",
      "translation": "",
      "startTime": 31.31,
      "endTime": 33.62
    },
    {
      "id": "s5",
      "role": "Narrator",
      "text": "What are you going to do with it?",
      "translation": "",
      "startTime": 33.63,
      "endTime": 37.38
    },
    {
      "id": "s6",
      "role": "Man",
      "text": "I'm going to put it here, in front of the window.",
      "translation": "",
      "startTime": 37.39,
      "endTime": 43.02
    },
    {
      "id": "s7",
      "role": "Woman",
      "text": "Be careful! Don't drop it!",
      "translation": "",
      "startTime": 43.03,
      "endTime": 47.31
    },
    {
      "id": "s8",
      "role": "Man",
      "text": "Don't put it there, Sam.",
      "translation": "",
      "startTime": 47.32,
      "endTime": 50.3
    },
    {
      "id": "s9",
      "role": "Woman",
      "text": "Put it here, on this shelf.",
      "translation": "",
      "startTime": 50.31,
      "endTime": 54.69
    },
    {
      "id": "s10",
      "role": "Man",
      "text": "There we are!",
      "translation": "",
      "startTime": 54.7,
      "endTime": 56.73
    },
    {
      "id": "s11",
      "role": "Woman",
      "text": "It's a lovely vase.",
      "translation": "",
      "startTime": 56.74,
      "endTime": 59.46
    },
    {
      "id": "s12",
      "role": "Man",
      "text": "Those flowers are lovely, too.",
      "translation": "",
      "startTime": 59.47,
      "endTime": 64.47
    }
  ]
}