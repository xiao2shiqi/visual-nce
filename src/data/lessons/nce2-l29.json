    },
    {
      "id": "intro_3",
      "role": "Narrator",
      "text": "The 'taxi' is a small Swiss aeroplane called a 'Pilatus Porter'.",
      "translation": "",
      "startTime": 23.34,
      "endTime": 29.68,
      "analysis": {
        "words": [
          {
            "word": "Taxi",
            "pos": "n.",
