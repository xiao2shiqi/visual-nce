{
  "id": "nce1-l89",
  "title": "Lesson 89: For Sale",
  "audio": "/audio/nce1/089&090－For Sale.mp3",
  "image": "/images/coming-soon.png",
  "segments": [
    {
      "id": "intro_1",
      "role": "Narrator",
      "text": "Lesson 89",
      "translation": "",
      "startTime": 0.6,
      "endTime": 2.98
    },
    {
      "id": "intro_2",
      "role": "Narrator",
      "text": "For sale",
      "translation": "",
      "startTime": 2.99,
      "endTime": 5.45
    },
    {
      "id": "intro_3",
      "role": "Narrator",
      "text": "Listen to the tape then answer this question.",
      "translation": "",
      "startTime": 5.46,
      "endTime": 11.19
    },
    {
      "id": "intro_4",
      "role": "Narrator",
      "text": "Why couldn't Nigel decide?",
      "translation": "",
      "startTime": 11.2,
      "endTime": 16.24
    },
    {
      "id": "s1",
      "role": "Man",
      "text": "Good afternoon.",
      "translation": "",
      "startTime": 16.25,
      "endTime": 18.35
    },
    {
      "id": "s2",
      "role": "Woman",
      "text": "I believe that this house is for sale.",
      "translation": "",
      "startTime": 18.36,
      "endTime": 22.73
    },
    {
      "id": "s3",
      "role": "Man",
      "text": "That's right.",
      "translation": "",
      "startTime": 22.74,
      "endTime": 24.52
    },
    {
      "id": "s4",
      "role": "Woman",
      "text": "May I have a look at it, please?",
      "translation": "",
      "startTime": 24.53,
      "endTime": 27.98
    },
    {
      "id": "s5",
      "role": "Man",
      "text": "Yes, of course.",
      "translation": "",
      "startTime": 27.99,
      "endTime": 29.73
    },
    {
      "id": "s6",
      "role": "Woman",
      "text": "Come in.",
      "translation": "",
      "startTime": 29.74,
      "endTime": 31.64
    },
    {
      "id": "s7",
      "role": "Narrator",
      "text": "How long have you lived here?",
      "translation": "",
      "startTime": 31.65,
      "endTime": 34.86
    },
    {
      "id": "s8",
      "role": "Man",
      "text": "I've lived here for twenty years.",
      "translation": "",
      "startTime": 34.87,
      "endTime": 38.27
    },
    {
      "id": "s9",
      "role": "Woman",
      "text": "Twenty years!",
      "translation": "",
      "startTime": 38.28,
      "endTime": 40.46
    },
    {
      "id": "s10",
      "role": "Man",
      "text": "That's a long time.",
      "translation": "",
      "startTime": 40.47,
      "endTime": 42.7
    },
    {
      "id": "s11",
      "role": "Woman",
      "text": "Yes, I've been here since 1976.",
      "translation": "",
      "startTime": 42.71,
      "endTime": 48.12
    },
    {
      "id": "s12",
      "role": "Man",
      "text": "Then why do you want to sell it?",
      "translation": "",
      "startTime": 48.13,
      "endTime": 51.81
    },
    {
      "id": "s13",
      "role": "Woman",
      "text": "Because I've just retired.",
      "translation": "",
      "startTime": 51.82,
      "endTime": 54.67
    },
    {
      "id": "s14",
      "role": "Man",
      "text": "I want to buy a small house in the country.",
      "translation": "",
      "startTime": 54.68,
      "endTime": 60.21
    },
    {
      "id": "s15",
      "role": "Narrator",
      "text": "How much does this house cost?",
      "translation": "",
      "startTime": 60.22,
      "endTime": 63.84
    },
    {
      "id": "s16",
      "role": "Man",
      "text": "￡68, 500.",
      "translation": "",
      "startTime": 63.85,
      "endTime": 68.34
    },
    {
      "id": "s17",
      "role": "Woman",
      "text": "That's a lot of money!",
      "translation": "",
      "startTime": 68.35,
      "endTime": 70.88
    },
    {
      "id": "s18",
      "role": "Man",
      "text": "It's worth every penny of it.",
      "translation": "",
      "startTime": 70.89,
      "endTime": 73.89
    },
    {
      "id": "s19",
      "role": "Woman",
      "text": "Well, I like the house, but I can't decide yet.",
      "translation": "",
      "startTime": 73.9,
      "endTime": 80.42
    },
    {
      "id": "s20",
      "role": "Man",
      "text": "My wife must see it first.",
      "translation": "",
      "startTime": 80.43,
      "endTime": 83.75
    },
    {
      "id": "s21",
      "role": "Woman",
      "text": "Women always have the last word.",
      "translation": "",
      "startTime": 83.76,
      "endTime": 88.76
    }
  ]
}