  "segments": [
    {
      "id": "intro_1",
      "role": "Narrator",
      "text": "Why will the police have a difficult time?",
      "translation": "",
      "startTime": 9.99,
      "endTime": 15.27
    },
    {
      "id": "intro_2",
      "role": "Narrator",
      "text": "The Greenwood Boys are a group of pop singers.",
      "translation": "",
      "startTime": 15.28,
      "endTime": 19.37
    },
    {
      "id": "intro_3",
      "role": "Narrator",
      "text": "At present, they are visiting all parts of the country.",
      "translation": "",
      "startTime": 19.38,
      "endTime": 24.47
    },
    {
      "id": "intro_4",
      "role": "Narrator",
      "text": "They will be arriving here tomorrow.",
      "translation": "",
      "startTime": 24.48,
      "endTime": 27.75
    },
    {
      "id": "s1",
      "role": "Man",
      "text": "They will be coming by train and most of the young people in the town will be meeting them at the station.",
      "translation": "",
      "startTime": 27.76,
      "endTime": 35.8
    },
    {
      "id": "s2",
      "role": "Woman",
      "text": "Tomorrow evening they will be singing at the Workers' Club.",
      "translation": "",
      "startTime": 35.81,
      "endTime": 40.81
    },
    {
      "id": "s3",
      "role": "Man",
      "text": "The Greenwood Boys will be staying for five days.",
      "translation": "",
      "startTime": 40.82,
      "endTime": 45.23
    },
    {
      "id": "s4",
      "role": "Woman",
      "text": "During this time, they will give five performances.",
      "translation": "",
      "startTime": 45.24,
      "endTime": 50.15
    },
    {
      "id": "s5",
      "role": "Man",
      "text": "As usual, the police will have a difficult time.",
      "translation": "",
      "startTime": 50.16,
      "endTime": 54.69
    },
    {
      "id": "s6",
      "role": "Woman",
      "text": "They will be trying to keep order.",
      "translation": "",
      "startTime": 54.7,
      "endTime": 57.55
    },
    {
      "id": "s7",
      "role": "Man",
      "text": "It is always the same on these occasions.",
      "translation": "",
      "startTime": 57.56,
      "endTime": 62.56
    }
  ]
}