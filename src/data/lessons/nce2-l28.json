    {
      "id": "intro_2",
      "role": "Narrator",
      "text": "Jasper White is one of those rare people who believes in ancient myths.",
      "translation": "",
      "startTime": 14.22,
      "endTime": 21.11,
      "analysis": {
        "words": [
          {
            "word": "White",
