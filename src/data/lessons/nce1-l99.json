    {
      "id": "intro_2",
      "role": "Narrator",
      "text": "Ow!",
      "translation": "",
      "startTime": 2.85,
      "endTime": 4.73
    },
    {
      "id": "intro_3",
      "role": "Narrator",
      "text": "Listen to the tape then answer this question.",
      "translation": "",
      "startTime": 4.74,
      "endTime": 10.48
    },
    {
      "id": "intro_4",
      "role": "Narrator",
      "text": "Must Andy go to see the doctor?",
      "translation": "",
      "startTime": 10.49,
      "endTime": 15.55
    },
    {
      "id": "s1",
      "role": "Man",
      "text": "Ow!",
      "translation": "",
      "startTime": 15.56,
      "endTime": 17.04
    },
    {
      "id": "s2",
      "role": "Narrator",
      "text": "What's the matter, Andy?",
      "translation": "",
      "startTime": 17.05,
      "endTime": 20.05
    },
    {
      "id": "s3",
      "role": "Man",
      "text": "I slipped and fell downstairs.",
      "translation": "",
      "startTime": 20.06,
      "endTime": 23.61
    },
    {
      "id": "s4",
      "role": "Woman",
      "text": "Have you hurt yourself?",
      "translation": "",
      "startTime": 23.62,
      "endTime": 26.45
    },
    {
      "id": "s5",
      "role": "Man",
      "text": "Yes, I have.",
      "translation": "",
      "startTime": 26.46,
      "endTime": 28.47
    },
    {
      "id": "s6",
      "role": "Woman",
      "text": "I think that I've hurt my back.",
      "translation": "",
      "startTime": 28.48,
      "endTime": 32.53
    },
    {
      "id": "s7",
      "role": "Man",
      "text": "Try and stand up.",
      "translation": "",
      "startTime": 32.54,
      "endTime": 35.28
    },
    {
      "id": "s8",
      "role": "Woman",
      "text": "Can you stand up?",
      "translation": "",
      "startTime": 35.29,
      "endTime": 37.44
    },
    {
      "id": "s9",
      "role": "Man",
      "text": "Here.",
      "translation": "",
      "startTime": 37.45,
      "endTime": 38.52
    },
    {
      "id": "s10",
      "role": "Woman",
      "text": "Let me help you.",
      "translation": "",
      "startTime": 38.53,
      "endTime": 41.5
    },
    {
      "id": "s11",
      "role": "Man",
      "text": "I'm sorry, Lucy.",
      "translation": "",
      "startTime": 41.51,
      "endTime": 43.98
    },
    {
      "id": "s12",
      "role": "Woman",
      "text": "I'm afraid that I can't get up.",
      "translation": "",
      "startTime": 43.99,
      "endTime": 48.03
    },
    {
      "id": "s13",
      "role": "Man",
      "text": "I think that the doctor had better see you.",
      "translation": "",
      "startTime": 48.04,
      "endTime": 52.96
    },
    {
      "id": "s14",
      "role": "Woman",
      "text": "I'll phone Dr. Carter.",
      "translation": "",
      "startTime": 52.97,
      "endTime": 56.96
    },
    {
      "id": "s15",
      "role": "Man",
      "text": "The doctor says that he will come at once.",
      "translation": "",
      "startTime": 56.97,
      "endTime": 61.82
    },
    {
      "id": "s16",
      "role": "Woman",
      "text": "I'm sure that you need an X-ray, Andy.",
      "translation": "",
      "startTime": 61.83,
      "endTime": 66.83
    }
  ]
