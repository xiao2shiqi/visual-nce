    {
      "id": "intro_1",
      "role": "Narrator",
      "text": "Why was the writer's aunt surprised?",
      "translation": "",
      "startTime": 10.47,
      "endTime": 15.99
    },
    {
      "id": "intro_2",
      "role": "Narrator",
      "text": "It was Sunday.",
      "translation": "",
      "startTime": 16,
      "endTime": 17.98,
      "analysis": {
        "words": [
          {
            "word": "It",
            "pos": "pron.",
            "meaning": "它"
