      "startTime": 244.92,
      "endTime": 255.25,
      "analysis": {
        "grammar": [
          {
            "text": "is",
            "label": "谓语/系动词"
          },
          {
            "text": "are",
            "label": "谓语/系动词"
          }
        ],
        "words": [
          {
            "word": "Is",
            "pos": "v.",
            "meaning": "是"
          }
        ]
      }
    },
