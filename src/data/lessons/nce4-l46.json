        "words": []
      }
    },
    {
      "id": "s19",
      "role": "Man",
      "text": "It is no use offering the manual labourer,",
      "translation": "",
      "startTime": 138.48,
      "endTime": 142.03,
      "analysis": {
        "grammar": [
          {
            "text": "It",
            "label": "人称代词/主语"
          },
          {
            "text": "is",
            "label": "谓语/系动词"
